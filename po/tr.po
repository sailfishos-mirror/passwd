#: libuser.c:88
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: libuser başlatma hatası:"

#: libuser.c:165
msgid "Warning: unlocked password would be empty."
msgstr "Uyarı: kilidi açık parola boş olacaktır."

#: libuser.c:279
msgid "Corrupted passwd entry."
msgstr "Bozuk passwd girdisi."

#: libuser.c:296
msgid "Empty password."
msgstr "Boş parola."

#: libuser.c:299
msgid "Password locked."
msgstr "Parola kilitli."

#: libuser.c:303
msgid "Password set, MD5 crypt."
msgstr "Parola dizisi, MD5 şifrelemeli."

#: libuser.c:306
msgid "Password set, blowfish crypt."
msgstr "Parola dizisi, Blowfish şifrelemeli."

#: libuser.c:309
msgid "Password set, SHA256 crypt."
msgstr "Parola dizisi, SHA256 şifrelemeli."

#: libuser.c:312
msgid "Password set, SHA512 crypt."
msgstr "Parola dizisi, SHA512 şifrelemeli."

#: libuser.c:314
msgid "Password set, unknown crypt variant."
msgstr "Parola dizisi, bilinmeyen şifreleme değişkenli."

#: libuser.c:317
msgid "Alternate authentication scheme in use."
msgstr "Alternatif kimlik doğrulama şeması kullanımda."

#: libuser.c:322
msgid "Password set, DES crypt."
msgstr "Parola dizisi, DES şifrelemeli."

#: libuser.c:336
#, c-format
msgid "No password set.\n"
msgstr "Parola belirlenmedi.\n"

#: libuser.c:340
#, c-format
msgid "Unknown user.\n"
msgstr "Bilinmeyen kullanıcı.\n"

#: libuser.c:435
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s: kullanıcı hesabında şifre yıllanması desteklenmiyor.\n"

#: passwd.c:156
msgid "keep non-expired authentication tokens"
msgstr "süresi sona ermemiş kimlik belirteçlerini tutar"

#: passwd.c:158
msgid "delete the password for the named account (root only)"
msgstr "ismi belirtilen hesabın parolasını siler (sadece root)"

#: passwd.c:160
msgid "lock the named account (root only)"
msgstr "ismi belirtilen hesabı kilitler (sadece root)"

#: passwd.c:162
msgid "unlock the named account (root only)"
msgstr "ismi belirtilen hesabın kilidini açar (sadece root)"

#: passwd.c:164
msgid "force operation"
msgstr "çalışmaya zorlar"

#: passwd.c:166
msgid "maximum password lifetime (root only)"
msgstr "gün olarak en fazla parola ömrü (saadece root)"

#: passwd.c:168
msgid "minimum password lifetime (root only)"
msgstr "gün olarak en düşük parola ömrü (sadece root)"

#: passwd.c:170
msgid "number of days warning users receives before password expiration (root only)"
msgstr "parola zaman aşımı öncesi kullanıcıların uyarı alacağı gün sayısı (sadece root)"

#: passwd.c:173
msgid "number of days after password expiration when an account becomes disabled (root only)"
msgstr "bir kullanıcı hesabı devre dışı kaldığında parola zaman aşımı sonrası gün sayısı (sadece root)"

#: passwd.c:176
msgid "report password status on the named account (root only)"
msgstr "ismi belirtilen hesaptaki parola durumunu bildirir (sadece root)"

#: passwd.c:178
msgid "read new tokens from stdin (root only)"
msgstr "stdin'den yeni belirteçleri oku (sadece root)"

#: passwd.c:185
msgid "[OPTION...] <accountName>"
msgstr "[SEÇENEK...] <Kullanıcı Adı>"

#: passwd.c:188
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: hatalı argüman %s: %s\n"

#: passwd.c:246
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s: -l, -u, -d, -S seçeneklerinden sadece biri belirtilebilir.\n"

#: passwd.c:255
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr "%s: -l, -u, -d, -S seçeneklerinden birisi ile -i, -n, -w, -x seçeneklerinden birisi beraber kullanılamaz.\n"

#: passwd.c:279
#, c-format
msgid "Only root can do that.\n"
msgstr "Sadece root yapabilir.\n"

#: passwd.c:292
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: Yeni bir kullanıcı adını sadece root belirleyebilir.\n"

#: passwd.c:301
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: Verilen kullanıcı adı çok uzun.\n"

#: passwd.c:311
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: Yalnızca bir kullanıcı adı belirtilebilir.\n"

#: passwd.c:321
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: Bu seçenek bir kullanıcı adı gerektirir.\n"

#: passwd.c:332
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: Kimliğiniz teşhis edilemedi!\n"

#: passwd.c:341
#: passwd.c:376
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: Bilinmeyen kullanıcı adı '%s'.\n"

#: passwd.c:393
#, c-format
msgid "%s: %s is not authorized to change the password of %s\n"
msgstr "%s: %s kullanıcısı %s kullanıcısının parolasını değiştirmeye yetkili değil\n"

#: passwd.c:406
#, c-format
msgid "Locking password for user %s.\n"
msgstr "%s kullanıcısı için parola kilitleniyor.\n"

#: passwd.c:410
#: passwd.c:422
#: passwd.c:436
#: passwd.c:456
msgid "Success"
msgstr "Başarılı"

#: passwd.c:410
#: passwd.c:425
msgid "Error (password not set?)"
msgstr "Hata (parola ayarlanmadı mı?)"

#: passwd.c:418
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "%s kullanıcısı için parolanın kilidi açılıyor.\n"

#: passwd.c:424
msgid "Unsafe operation (use -f to force)"
msgstr "Kararsız çalışma (zorlamak için -f kullanın)"

#: passwd.c:433
#, c-format
msgid "Removing password for user %s.\n"
msgstr "%s kullanıcısının parolası kaldırılıyor.\n"

#: passwd.c:436
#: passwd.c:456
msgid "Error"
msgstr "Hata"

#: passwd.c:453
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "%s kullanıcısı için yıllanma verisi ayarlanıyor.\n"

#: passwd.c:469
#, c-format
msgid "Changing password for user %s.\n"
msgstr "%s kullanıcısının şifresi değiştiriliyor.\n"

#: passwd.c:481
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: stdin'den okuma hatası: %s\n"

#: passwd.c:498
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: pam başlatamıyor: %s\n"

#: passwd.c:511
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: pam için tty ayarlanamıyor: %s\n"

#: passwd.c:522
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: başarısızlık gecikmesi ayarlanamıyor: %s\n"

#: passwd.c:540
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s: süresi dolmuş kimlik belirteçleri başarıyla güncellendi.\n"

#: passwd.c:546
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: tüm kimlik doğrulama belirteçleri başarıyla güncellendi.\n"

