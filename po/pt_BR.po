#: libuser.c:88
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: erro de inicialização da libuser:"

#: libuser.c:165
msgid "Warning: unlocked password would be empty."
msgstr "Aviso: a senha desbloqueada ficaria vazia."

#: libuser.c:279
msgid "Corrupted passwd entry."
msgstr "Entrada de senha corrompida."

#: libuser.c:296
msgid "Empty password."
msgstr "Senha vazia."

#: libuser.c:299
msgid "Password locked."
msgstr "Senha bloqueada."

#: libuser.c:303
msgid "Password set, MD5 crypt."
msgstr "Definição de senha, criptografia MD5."

#: libuser.c:306
msgid "Password set, blowfish crypt."
msgstr "Definição de senha, criptografia blowfish."

#: libuser.c:309
msgid "Password set, SHA256 crypt."
msgstr "Definição de senha, criptografia SHA256."

#: libuser.c:312
msgid "Password set, SHA512 crypt."
msgstr "Definição de senha, criptografia SHA512."

#: libuser.c:314
msgid "Password set, unknown crypt variant."
msgstr "Definição de senha, variação desconhecida de criptografia."

#: libuser.c:317
msgid "Alternate authentication scheme in use."
msgstr "Alternar esquema de autenticação em uso."

#: libuser.c:322
msgid "Password set, DES crypt."
msgstr "Definição de senha, criptografia DES."

#: libuser.c:336
#, c-format
msgid "No password set.\n"
msgstr "Sem definição de senha.\n"

#: libuser.c:340
#, c-format
msgid "Unknown user.\n"
msgstr "Usuário desconhecido.\n"

#: libuser.c:435
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s: a conta do usuário não tem suporte à expiração de senha.\n"

#: passwd.c:156
msgid "keep non-expired authentication tokens"
msgstr "manter tokens de autenticação não-expirados"

#: passwd.c:158
msgid "delete the password for the named account (root only)"
msgstr "remover senha para a conta mencionada (somente root)"

#: passwd.c:160
msgid "lock the named account (root only)"
msgstr "bloquear a conta mencionada (somente root)"

#: passwd.c:162
msgid "unlock the named account (root only)"
msgstr "desbloquear a conta mencionada (somente root)"

#: passwd.c:164
msgid "force operation"
msgstr "forçar operação"

#: passwd.c:166
msgid "maximum password lifetime (root only)"
msgstr "tempo máximo de vida útil da senha (somente root)"

#: passwd.c:168
msgid "minimum password lifetime (root only)"
msgstr "tempo mínimo de vida útil da senha (somente root)"

#: passwd.c:170
msgid ""
"number of days warning users receives before password expiration (root only)"
msgstr ""
"número de dias para avisar o usuário antes da expiração da senha (somente "
"root)"

#: passwd.c:173
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr ""
"número de dias após a expiração da senha para que a conta se torne "
"desabilitada (somente root)"

#: passwd.c:176
msgid "report password status on the named account (root only)"
msgstr "reportar status da senha na conta mencionada (somente root)"

#: passwd.c:178
msgid "read new tokens from stdin (root only)"
msgstr "ler novos tokens a partir do stdin (somente root)"

#: passwd.c:185
msgid "[OPTION...] <accountName>"
msgstr "[OPÇÃO...] <Nome_da_conta>"

#: passwd.c:188
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: argumento inválido %s: %s\n"

#: passwd.c:246
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s: Apenas um -l, -u, -d, - S pode ser especificado.\n"

#: passwd.c:255
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr "%s: Não é possível misturar um -l, -u, -d, -S e um -i, -n, -w, -x.\n"

#: passwd.c:279
#, c-format
msgid "Only root can do that.\n"
msgstr "Apenas o root pode fazer isso.\n"

#: passwd.c:292
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: Somente o root pode especificar um nome de usuário.\n"

#: passwd.c:301
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: O nome de usuário fornecido é muito longo.\n"

#: passwd.c:311
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: Somente um nome de usuário pode ser especificado.\n"

#: passwd.c:321
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: Esta opção requer um nome de usuário.\n"

#: passwd.c:332
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: Não foi possível identificar você!\n"

#: passwd.c:341 passwd.c:376
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: Nome de usuário \"%s\" desconhecido.\n"

#: passwd.c:393
#, c-format
msgid "%s: %s is not authorized to change the password of %s\n"
msgstr "%s: %s não está autorizado a mudar a senha de %s\n"

#: passwd.c:406
#, c-format
msgid "Locking password for user %s.\n"
msgstr "Bloqueando senha para o usuário %s.\n"

#: passwd.c:410 passwd.c:422 passwd.c:436 passwd.c:456
msgid "Success"
msgstr "Sucesso"

#: passwd.c:410 passwd.c:425
msgid "Error (password not set?)"
msgstr "Erro (senha não definida?)"

#: passwd.c:418
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "Desbloqueando senha para o usuário %s.\n"

#: passwd.c:424
msgid "Unsafe operation (use -f to force)"
msgstr "Operação insegura (use -f para forçar)"

#: passwd.c:433
#, c-format
msgid "Removing password for user %s.\n"
msgstr "Removendo senha para o usuário %s.\n"

#: passwd.c:436 passwd.c:456
msgid "Error"
msgstr "Erro"

#: passwd.c:453
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "Ajustando dados de expiração para o usuário %s.\n"

#: passwd.c:469
#, c-format
msgid "Changing password for user %s.\n"
msgstr "Mudando senha para o usuário %s.\n"

#: passwd.c:481
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: erro ao ler do stdin: %s\n"

#: passwd.c:498
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: não foi possível iniciar o pam: %s\n"

#: passwd.c:511
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: não foi possível definir um tty para o pam: %s\n"

#: passwd.c:522
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: não foi possível definir o atraso de falha: %s\n"

#: passwd.c:540
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s: tokens de autenticações expiradas atualizados com sucesso.\n"

#: passwd.c:546
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: todos os tokens de autenticações foram atualizados com sucesso.\n"
