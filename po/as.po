#: libuser.c:88
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: libuser আৰম্ভ কৰাত ভুল:"

#: libuser.c:165
msgid "Warning: unlocked password would be empty."
msgstr "সকিয়নী: বন্ধ নকৰা গুপ্তশব্দ ৰিক্ত হ'ব ।"

#: libuser.c:279
msgid "Corrupted passwd entry."
msgstr "দূষিত গুপ্তশব্দৰ নিবেশ ।"

#: libuser.c:296
msgid "Empty password."
msgstr "ৰিক্ত গুপ্তশব্দ ।"

#: libuser.c:299
msgid "Password locked."
msgstr "গুপ্তশব্দ বন্ধ কৰা হ'ল ।"

#: libuser.c:303
msgid "Password set, MD5 crypt."
msgstr "গুপ্তশব্দ নিৰ্দ্ধাৰিত কৰা হ'ল, MD5 এৰে গুপুত কৰা হ'ল ।"

#: libuser.c:306
msgid "Password set, blowfish crypt."
msgstr "গুপ্তশব্দ নিৰ্দ্ধাৰিত কৰা হ'ল, blowfish এৰে গুপুত কৰা হ'ল ।"

#: libuser.c:309
msgid "Password set, SHA256 crypt."
msgstr "গুপ্তশব্দ নিৰ্দ্ধাৰিত কৰা হ'ল, SHA256 এৰে গুপুত কৰা হ'ল ।"

#: libuser.c:312
msgid "Password set, SHA512 crypt."
msgstr "গুপ্তশব্দ নিৰ্দ্ধাৰিত কৰা হ'ল, SHA512 এৰে গুপুত কৰা হ'ল ।"

#: libuser.c:314
msgid "Password set, unknown crypt variant."
msgstr "গুপ্তশব্দ নিৰ্দ্ধাৰিত কৰা হ'ল, অজ্ঞাত গুপুত কৰা বিভেদক ।"

#: libuser.c:317
msgid "Alternate authentication scheme in use."
msgstr "বৈকল্পপিক প্ৰমাণীকৰণৰ আঁচনি ব্যৱহাৰত আছে ।"

#: libuser.c:322
msgid "Password set, DES crypt."
msgstr "গুপ্তশব্দ নিৰ্দ্ধাৰিত কৰা হ'ল, DES এৰে গুপুত কৰা হ'ল ।"

#: libuser.c:336
#, c-format
msgid "No password set.\n"
msgstr "কোনো গুপ্তশবদ নিৰ্দ্ধাৰিত কৰা হোৱা নাই ।\n"

#: libuser.c:340
#, c-format
msgid "Unknown user.\n"
msgstr "অজ্ঞাত ব্যৱহাৰকৰোঁতা ।\n"

#: libuser.c:435
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s: ব্যৱহাৰকৰোঁতাৰ হিচাপত গুপ্তশব্দ পুৰণি হোৱাৰ সমৰ্থন নাই ।\n"

#: passwd.c:156
msgid "keep non-expired authentication tokens"
msgstr "প্ৰমাণীকৰণৰ অসম্পূৰ্ণ্চিহ্নকেনৰ সংক কৰা হ'ব হবে"

#: passwd.c:158
msgid "delete the password for the named account (root only)"
msgstr "উল্লেখিত হিচাপৰ গুপ্তশব্দ আঁতৰাওক (অকল root)"

#: passwd.c:160
msgid "lock the named account (root only)"
msgstr "উল্লেখিত হিচাপ বন্ধ কৰক (অকল root)"

#: passwd.c:162
msgid "unlock the named account (root only)"
msgstr "উল্লেখিত হিচাপ আন-লক কৰক (অকল root)"

#: passwd.c:164
msgid "force operation"
msgstr "কাৰ্য্য বলবৎ কৰক"

#: passwd.c:166
msgid "maximum password lifetime (root only)"
msgstr "গুপ্তশব্দৰ সৰ্বাধিক আয়ুঃকাল (অকল root)"

#: passwd.c:168
msgid "minimum password lifetime (root only)"
msgstr "গুপ্তশব্দৰ সৰ্বনিম্ন আয়ুঃকাল (অকল root)"

#: passwd.c:170
msgid ""
"number of days warning users receives before password expiration (root only)"
msgstr ""
"গুপ্তশব্দৰ আয়ুঃকালপূৰ্তীৰ কেইদিন পূৰ্বে ব্যৱহাৰকৰোঁতাক সতৰ্কবাৰ্তা দিয়া হ'ব (অকল root)"

#: passwd.c:173
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr "গুপ্তশব্দৰ আয়ুঃকালপূৰ্তীৰ কেইদিনৰ পিছত হিচাপ নিষ্ক্ৰিয় কৰা হ'ব (অকল root)"

#: passwd.c:176
msgid "report password status on the named account (root only)"
msgstr "উল্লেখিত হিচাপৰ গুপ্তশব্দৰ অৱস্থা বৰ্ণনা কৰক (অকল root)"

#: passwd.c:178
msgid "read new tokens from stdin (root only)"
msgstr "stdin ৰ পৰা নতুন টোকেন পঢ়ক (অকল root)"

#: passwd.c:185
msgid "[OPTION...] <accountName>"
msgstr "[OPTION...] <accountName>"

#: passwd.c:188
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: ভুল তৰ্ক %s: %s\n"

#: passwd.c:246
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s: -l, -u, -d, -S ৰ যি কোনো এটা প্ৰয়োগ কৰা যাব পাৰে ।\n"

#: passwd.c:255
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr ""
"%s: -l, -u, -d, -S ৰ যি কোনো বিকল্পৰ সৈতে -i, -n, -w, -x সংকলনৰ কোনো বিকল্প "
"যৌথৰূপে প্ৰয়োগ কৰিব নোৱাৰে ।\n"

#: passwd.c:279
#, c-format
msgid "Only root can do that.\n"
msgstr "অকল root এ সেইটো কৰিব পাৰে ।\n"

#: passwd.c:292
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: অকল root এ ব্যৱহাৰকৰোঁতাৰ নাম উল্লেখ কৰিব পাৰে ।\n"

#: passwd.c:301
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: উল্লেখিত ব্যৱহাৰকৰোঁতাৰ নাম বৰ দীঘল ।\n"

#: passwd.c:311
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: অকল এজন ব্যৱহাৰকৰোঁতাৰ নাম উল্লেখ কৰা যাব পাৰে ।\n"

#: passwd.c:321
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: এই বিকল্পৰ সৈতে ব্যৱহাৰকৰোঁতাৰ নাম উল্লেখ কৰা আৱশ্যক ।\n"

#: passwd.c:332
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: আপনাকে চিনাক্ত কৰা নহ'ল!\n"

#: passwd.c:341 passwd.c:376
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: অজ্ঞাত ব্যৱহাৰকৰোঁতাৰ নাম '%s'।\n"

#: passwd.c:393
#, c-format
msgid "%s: %s is not authorized to change the password of %s\n"
msgstr "%s: %s, %s ৰ গুপ্তশব্দ পৰিবৰ্ত্তনৰ অনুমতি নাই \n"

#: passwd.c:406
#, c-format
msgid "Locking password for user %s.\n"
msgstr "ব্যৱহাৰকৰোঁতা %s ৰ বাবে গুপ্তশব্দ বন্ধ কৰা হৈছে ।\n"

#: passwd.c:410 passwd.c:422 passwd.c:436 passwd.c:456
msgid "Success"
msgstr "সফল"

#: passwd.c:410 passwd.c:425
msgid "Error (password not set?)"
msgstr "ভুল (গুপ্তশব্দ প্ৰতিষ্ঠা কৰা হোৱা নাই?)"

#: passwd.c:418
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "ব্যৱহাৰকৰোঁতা %s ৰ গুপ্তশব্দ আন-লক কৰা হৈছে ।\n"

#: passwd.c:424
msgid "Unsafe operation (use -f to force)"
msgstr "বিপজ্জনক কৰ্ম (বলবৎভাবে প্ৰয়োগ কৰিব'লৈ -f ব্যবহাৰ কৰক)"

#: passwd.c:433
#, c-format
msgid "Removing password for user %s.\n"
msgstr "%s ব্যৱহাৰকৰোঁতাৰ গুপ্তশব্দ আঁতৰুৱা হৈছে ।\n"

#: passwd.c:436 passwd.c:456
msgid "Error"
msgstr "ভুল"

#: passwd.c:453
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "%s ব্যৱহাৰকৰোঁতাৰ বাবে আয়ুঃকালপূৰ্তী সংক্ৰান্ত তথ্য পৰিবৰ্ত্তন কৰা হৈছে ।\n"

#: passwd.c:469
#, c-format
msgid "Changing password for user %s.\n"
msgstr "%s ব্যৱহাৰকৰোঁতাৰ গুপ্তশব্দ পৰিবৰ্ত্তন কৰা হৈছে ।\n"

#: passwd.c:481
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: stdin ৰ পৰা পঢ়োঁতে সমস্যা: %s\n"

#: passwd.c:498
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: pam আৰম্ভ কৰাত ব্যৰ্থ: %s\n"

#: passwd.c:511
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: pam ৰ বাবে tty নিৰ্দ্ধাৰণ কৰাত ব্যৰ্থ: %s\n"

#: passwd.c:522
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: বিফলতাৰ সময়কাল নিৰ্দ্ধাৰণ কৰাত ব্যৰ্থ: %s\n"

#: passwd.c:540
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s: আয়ুঃকাল সমাপ্ত অনুমোদনৰ চিহ্ন সফলতাৰে উন্নয়ন কৰা হৈছে ।\n"

#: passwd.c:546
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: সমস্ত অনুমোদনৰ চিহ্ন সফলতাৰে উন্নয়ন কৰা হৈছে ।\n"
