#: libuser.c:88
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: грешка при инициализация на libuser:"

#: libuser.c:165
msgid "Warning: unlocked password would be empty."
msgstr "Внимание: отключената парола ще бъде празна."

#: libuser.c:279
msgid "Corrupted passwd entry."
msgstr "Повреден запис за парола."

#: libuser.c:296
msgid "Empty password."
msgstr "Празна парола."

#: libuser.c:299
msgid "Password locked."
msgstr "Заключена парола."

#: libuser.c:303
msgid "Password set, MD5 crypt."
msgstr "Зададена парола, MD5 криптиране."

#: libuser.c:306
msgid "Password set, blowfish crypt."
msgstr "Зададена парола, blowfish криптиране."

#: libuser.c:309
#, fuzzy
msgid "Password set, SHA256 crypt."
msgstr "Зададена парола, MD5 криптиране."

#: libuser.c:312
#, fuzzy
msgid "Password set, SHA512 crypt."
msgstr "Зададена парола, MD5 криптиране."

#: libuser.c:314
msgid "Password set, unknown crypt variant."
msgstr "Зададена парола, неизвестно криптиране."

#: libuser.c:317
msgid "Alternate authentication scheme in use."
msgstr "Ползва се алтернативна схема за удостоверяване."

#: libuser.c:322
msgid "Password set, DES crypt."
msgstr "Зададена парола, DES крпитиране."

#: libuser.c:336
#, c-format
msgid "No password set.\n"
msgstr "Не е зададена парола.\n"

#: libuser.c:340
#, c-format
msgid "Unknown user.\n"
msgstr "Неизвестен потребител.\n"

#: libuser.c:435
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s: потребителския акаунт няма поддръжка за стареене на паролата.\n"

#: passwd.c:156
msgid "keep non-expired authentication tokens"
msgstr "пазене на неизтеклите удостоверяващи полета"

#: passwd.c:158
msgid "delete the password for the named account (root only)"
msgstr "изтриване на паролата за указан акаунт (само root)"

#: passwd.c:160
msgid "lock the named account (root only)"
msgstr "заключване на указан акаунт (само root)"

#: passwd.c:162
msgid "unlock the named account (root only)"
msgstr "отключване на указан акаунт (само root)"

#: passwd.c:164
msgid "force operation"
msgstr "насилване на операцията"

#: passwd.c:166
msgid "maximum password lifetime (root only)"
msgstr "максимален живот на паролата (само root)"

#: passwd.c:168
msgid "minimum password lifetime (root only)"
msgstr "минимален живот на паролата (само root)"

#: passwd.c:170
msgid ""
"number of days warning users receives before password expiration (root only)"
msgstr ""
"брой дни които потребителя получава предупреждение за изтичане на паролата "
"(само root)"

#: passwd.c:173
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr ""
"брой дни след изтичане на паролата след които акаунта бива забранен (само "
"root)"

#: passwd.c:176
msgid "report password status on the named account (root only)"
msgstr "рапорт за състоянието на паролата на указан акаунт (само root)"

#: passwd.c:178
msgid "read new tokens from stdin (root only)"
msgstr "четене на новите полета от стандартния вход (само root)"

#: passwd.c:185
msgid "[OPTION...] <accountName>"
msgstr "[ПАРАМЕТЪР...] <имеНаАкаунт>"

#: passwd.c:188
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: лош аргумент %s: %s\n"

#: passwd.c:246
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s: Само един от ключовете -l, -u, -d, -S може да бъде указан.\n"

#: passwd.c:255
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr "%s: Не могат да се смесват -l, -u, -d, -S с един от -i, -n, -w, -x.\n"

#: passwd.c:279
#, c-format
msgid "Only root can do that.\n"
msgstr "Само потребителя root може да прави това.\n"

#: passwd.c:292
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: Само потребителя root може да указва потребителско име.\n"

#: passwd.c:301
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: Въведеното потребителско име е твърде дълго.\n"

#: passwd.c:311
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: Може да бъде указано само едно потребителско име.\n"

#: passwd.c:321
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: Тази опция изисква потребителско име.\n"

#: passwd.c:332
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: Не може да Ви идентифицира!\n"

#: passwd.c:341 passwd.c:376
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: Неизвестно потребителско име '%s'.\n"

#: passwd.c:393
#, c-format
msgid "%s: %s is not authorized to change the password of %s\n"
msgstr "%s: %s не му е позволено да променя паролата на %s\n"

#: passwd.c:406
#, c-format
msgid "Locking password for user %s.\n"
msgstr "Заключване паролата на потребител %s.\n"

#: passwd.c:410 passwd.c:422 passwd.c:436 passwd.c:456
msgid "Success"
msgstr "Успех"

#: passwd.c:410 passwd.c:425
msgid "Error (password not set?)"
msgstr "Грешка (паролата не е установена?)"

#: passwd.c:418
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "Отключване паролата на потребител %s.\n"

#: passwd.c:424
msgid "Unsafe operation (use -f to force)"
msgstr "Рискова операция (ползвайте -f за да я насилите)"

#: passwd.c:433
#, c-format
msgid "Removing password for user %s.\n"
msgstr "Махане паролата на потребител %s.\n"

#: passwd.c:436 passwd.c:456
msgid "Error"
msgstr "Грешка"

#: passwd.c:453
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "Донастройване данните за стареене за потребител %s.\n"

#: passwd.c:469
#, c-format
msgid "Changing password for user %s.\n"
msgstr "Промяна паролата на потребител %s.\n"

#: passwd.c:481
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: грешка при четене от стандартния вход: %s\n"

#: passwd.c:498
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: не може да бъде стартиран pam: %s\n"

#: passwd.c:511
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: не може да бъде настроено tty за параметър: %s\n"

#: passwd.c:522
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: не може да се установи забавянето при неуспех: %s\n"

#: passwd.c:540
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s: изтеклите полета за удостоверяване бяха обновени успешно.\n"

#: passwd.c:546
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: всички полета за удостоверяване бяха обновени успешно.\n"
