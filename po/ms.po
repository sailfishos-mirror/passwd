#: passwd.c:185
msgid "[OPTION...] <accountName>"
msgstr "[PILIHAN...] <namaAkaun>"

#: passwd.c:188
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: hujah teruk %s: %s\n"

#: passwd.c:246
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s: Hanya satu dari -l, -u, -d, -S boleh dinyatakan.\n"

#: passwd.c:255
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr ""
"%s: Tidak dapat mencampurkan salah satu dari -l, -u, -d, -S dan satu dari -"
"i, -n, -w, -x.\n"

#: passwd.c:279
#, c-format
msgid "Only root can do that.\n"
msgstr "Hanya root boleh melakukannya.\n"

#: passwd.c:292
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: Hanya root boleh menyatakan nama pengguna.\n"

#: passwd.c:301
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: Nama pengguna yang diberikan terlalu panjang.\n"

#: passwd.c:311
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: Hanya satu nama pengguna boleh dinyatakan.\n"

#: passwd.c:321
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: Pilihan ini memerlukan nama pengguna.\n"

#: passwd.c:332
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: Tidak dapat mengenalpasti anda!\n"

#: passwd.c:341 passwd.c:376
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: Nama pengguna '%s' tidak diketahui.\n"

#: passwd.c:393
#, c-format
msgid "%s: %s is not authorized to change the password of %s\n"
msgstr "%s: %s tidak dibenarkan untuk mengubah kata laluan bagi %s\n"

#: passwd.c:406
#, c-format
msgid "Locking password for user %s.\n"
msgstr "Mengunci katalaluan untuk pengguna %s.\n"

#: passwd.c:410 passwd.c:422 passwd.c:436 passwd.c:456
msgid "Success"
msgstr "Berjaya"

#: passwd.c:410 passwd.c:425
msgid "Error (password not set?)"
msgstr "Ralat (katalaluan tidak ditetapkan?)"

#: passwd.c:418
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "Menyahkunci katalaluan untuk pengguna %s.\n"

#: passwd.c:424
msgid "Unsafe operation (use -f to force)"
msgstr "Operasi tidak selamat (guna -f untuk paksa)"

#: passwd.c:433
#, c-format
msgid "Removing password for user %s.\n"
msgstr "Membuang katalaluan untuk pengguna %s.\n"

#: passwd.c:436 passwd.c:456
msgid "Error"
msgstr "Ralat"

#: passwd.c:453
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "Mengubahsuai data penuaan untuk pengguna %s.\n"

#: passwd.c:469
#, c-format
msgid "Changing password for user %s.\n"
msgstr "Menukar katalaluan untuk pengguna %s.\n"

#: passwd.c:481
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: ralat membaca daripada stdin: %s\n"

#: passwd.c:498
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: tidak dapat memulakan pam: %s\n"

#: passwd.c:511
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: tidak dapat menetapkan tty untuk pam: %s\n"

#: passwd.c:522
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: tidak dapat menetapkan lengahan kegagalan: %s\n"

#: passwd.c:540
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s: token pengesahan tamat tempoh telah berjaya dikemaskini.\n"

#: passwd.c:546
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: semua token pengesahan telah berjaya dikemaskini.\n"
