#: libuser.c:88
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: błąd inicjowania libuser:"

#: libuser.c:165
msgid "Warning: unlocked password would be empty."
msgstr "Ostrzeżenie: niezablokowane hasło może być puste."

#: libuser.c:279
msgid "Corrupted passwd entry."
msgstr "Uszkodzony wpis passwd."

#: libuser.c:296
msgid "Empty password."
msgstr "Puste hasło."

#: libuser.c:299
msgid "Password locked."
msgstr "Hasło jest zablokowane."

#: libuser.c:303
msgid "Password set, MD5 crypt."
msgstr "Hasło zostało ustawione, szyfrowane MD5."

#: libuser.c:306
msgid "Password set, blowfish crypt."
msgstr "Hasło zostało ustawione, szyfrowane blowfish."

#: libuser.c:309
msgid "Password set, SHA256 crypt."
msgstr "Hasło zostało ustawione, szyfrowane SHA256."

#: libuser.c:312
msgid "Password set, SHA512 crypt."
msgstr "Hasło zostało ustawione, szyfrowane SHA512."

#: libuser.c:314
msgid "Password set, unknown crypt variant."
msgstr "Hasło zostało ustawione, nieznany wariant szyfrowania."

#: libuser.c:317
msgid "Alternate authentication scheme in use."
msgstr "Używany jest alternatywny schemat uwierzytelniania."

#: libuser.c:322
msgid "Password set, DES crypt."
msgstr "Hasło zostało ustawione, szyfrowane DES."

#: libuser.c:336
#, c-format
msgid "No password set.\n"
msgstr "Nie ustawiono hasła.\n"

#: libuser.c:340
#, c-format
msgid "Unknown user.\n"
msgstr "Nieznany użytkownik.\n"

#: libuser.c:435
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s: konto użytkownika nie obsługuje wygasania hasła.\n"

#: passwd.c:156
msgid "keep non-expired authentication tokens"
msgstr "zatrzymaj niewygasłe tokeny uwierzytelniania"

#: passwd.c:158
msgid "delete the password for the named account (root only)"
msgstr "usuń hasło dla danego konta (tylko root)"

#: passwd.c:160
msgid "lock the named account (root only)"
msgstr "zablokuj dane konto (tylko root)"

#: passwd.c:162
msgid "unlock the named account (root only)"
msgstr "odblokuj dane konto (tylko root)"

#: passwd.c:164
msgid "force operation"
msgstr "wymuś operację"

#: passwd.c:166
msgid "maximum password lifetime (root only)"
msgstr "maksymalny czas ważności hasła (tylko root)"

#: passwd.c:168
msgid "minimum password lifetime (root only)"
msgstr "minimalny czas ważności hasła (tylko root)"

#: passwd.c:170
msgid ""
"number of days warning users receives before password expiration (root only)"
msgstr ""
"liczba dni ostrzegania użytkownika przed wygaśnięciem hasła (tylko root)"

#: passwd.c:173
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr ""
"liczba dni po wygaśnięciu hasła, kiedy konto zostanie wyłączone (tylko root)"

#: passwd.c:176
msgid "report password status on the named account (root only)"
msgstr "zgłoś stan hasła dla danego konta (tylko root)"

#: passwd.c:178
msgid "read new tokens from stdin (root only)"
msgstr "odczytaj nowe tokeny ze standardowego wejścia (tylko root)"

#: passwd.c:185
msgid "[OPTION...] <accountName>"
msgstr "[OPCJA...] <nazwaKonta>"

#: passwd.c:188
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: błędny argument %s: %s\n"

#: passwd.c:246
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s: może być podany tylko jeden z -l, -u, -d lub -S.\n"

#: passwd.c:255
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr ""
"%s: nie można łączyć jednego z -l, -u, -d i -S oraz jednego z -i, -n, -w i -"
"x.\n"

#: passwd.c:279
#, c-format
msgid "Only root can do that.\n"
msgstr "Tylko root może to zrobić.\n"

#: passwd.c:292
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: tylko root może podać nazwę konta.\n"

#: passwd.c:301
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: podana nazwa użytkownika jest za długa.\n"

#: passwd.c:311
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: może zostać podana tylko jedna nazwa użytkownika.\n"

#: passwd.c:321
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: ta opcja wymaga nazwy użytkownika.\n"

#: passwd.c:332
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: nie można cię zidentyfikować!\n"

#: passwd.c:341 passwd.c:376
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: nieznana nazwa użytkownika \"%s\".\n"

#: passwd.c:393
#, c-format
msgid "%s: %s is not authorized to change the password of %s\n"
msgstr "%s: %s jest nieupoważniony do zmiany hasła %s\n"

#: passwd.c:406
#, c-format
msgid "Locking password for user %s.\n"
msgstr "Blokowanie hasła użytkownika %s.\n"

#: passwd.c:410 passwd.c:422 passwd.c:436 passwd.c:456
msgid "Success"
msgstr "Powodzenie"

#: passwd.c:410 passwd.c:425
msgid "Error (password not set?)"
msgstr "Błąd (hasło nie jest ustawione?)"

#: passwd.c:418
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "Odblokowywanie hasła użytkownika %s.\n"

#: passwd.c:424
msgid "Unsafe operation (use -f to force)"
msgstr "Niebezpieczna operacja (użyj -f, aby wymusić)"

#: passwd.c:433
#, c-format
msgid "Removing password for user %s.\n"
msgstr "Usuwanie hasła użytkownika %s.\n"

#: passwd.c:436 passwd.c:456
msgid "Error"
msgstr "Błąd"

#: passwd.c:453
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "Przystosowywanie danych wygasania użytkownika %s.\n"

#: passwd.c:469
#, c-format
msgid "Changing password for user %s.\n"
msgstr "Zmienianie hasła użytkownika %s.\n"

#: passwd.c:481
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: błąd podczas odczytywania ze standardowego wejścia: %s\n"

#: passwd.c:498
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: nie można uruchomić PAM: %s\n"

#: passwd.c:511
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: nie można ustawić TTY dla PAM: %s\n"

#: passwd.c:522
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: nie można ustawić opóźnienia odmowy: %s\n"

#: passwd.c:540
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s: zaktualizowanie wygasłych tokenów uwierzytelniania powiodło się.\n"

#: passwd.c:546
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr ""
"%s: zaktualizowanie wszystkich tokenów uwierzytelniania powiodło się.\n"
