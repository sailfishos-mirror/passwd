msgid "[OPTION...] <accountName>"
msgstr "[VOĽBA...] <názovKonta>"

#: passwd.c:192
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: nesprávny argument %s: %s\n"

