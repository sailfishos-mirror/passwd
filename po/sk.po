#: libuser.c:90
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: chyba inicializácie libuser:"

#: libuser.c:167
msgid "Warning: unlocked password would be empty."
msgstr "Varovanie: odomknuté heslo bude prázdne."

#: libuser.c:281
msgid "Corrupted passwd entry."
msgstr "Poškodená položka passwd."

#: libuser.c:298
msgid "Empty password."
msgstr "Prázdne heslo."

#: libuser.c:301
msgid "Password locked."
msgstr "Heslo zamknuté."

#: libuser.c:305
msgid "Password set, MD5 crypt."
msgstr "Heslo nastavené,š ifra MD5."

#: libuser.c:308
msgid "Password set, blowfish crypt."
msgstr "Heslo nastavené, šifra blowfish."

#: libuser.c:310
msgid "Password set, unknown crypt variant."
msgstr "Heslo nastavené, neznámy variant šifry."

#: libuser.c:313
msgid "Alternate authentication scheme in use."
msgstr "Použitá alternatívna autentifikačná schéma."

#: libuser.c:318
msgid "Password set, DES crypt."
msgstr "Heslo nastavené, šifra DES."

#: libuser.c:332
#, c-format
msgid "No password set.\n"
msgstr "Žiadné heslo nebolo nastavené.\n"

#: libuser.c:336
#, c-format
msgid "Unknown user.\n"
msgstr "Neznámy užívateľ.\n"

#: libuser.c:431
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s: užívateľské konto nemá podporu pre stárnutie hesla.\n"

#: passwd.c:158
msgid "keep non-expired authentication tokens"
msgstr "ponechať nevypršané autentifikačné kľúče"

#: passwd.c:160
msgid "delete the password for the named account (root only)"
msgstr "zmazať heslo pre konto named (iba správca)"

#: passwd.c:162
msgid "lock the named account (root only)"
msgstr "zamknúť konto named (iba správca)"

#: passwd.c:164
msgid "unlock the named account (root only)"
msgstr "odomknúť konto named (iba správca)"

#: passwd.c:166
msgid "force operation"
msgstr "vnútiť operáciu"

#: passwd.c:168
msgid "maximum password lifetime (root only)"
msgstr "najdlhšia životnosť hesla (iba správca)"

#: passwd.c:170
msgid "minimum password lifetime (root only)"
msgstr "najkratšia životnosť hesla (iba správca)"

#: passwd.c:172
msgid "number of days warning users receives before password expiration (root only)"
msgstr "počet dní pred vypršaním hesla kedy začať varovať užívateľov (iba správca)"

#: passwd.c:175
msgid "number of days after password expiration when an account becomes disabled (root only)"
msgstr "počet dní po vypršaní hesla kedy bude konto zablokované (iba správca)"

#: passwd.c:178
msgid "report password status on the named account (root only)"
msgstr "ohlásiť stav hesla na konte named (iba správca)"

#: passwd.c:180
msgid "read new tokens from stdin (root only)"
msgstr "načítať nové kľúče z stdin (iba správca)"

#: passwd.c:187
msgid "[OPTION...] <accountName>"
msgstr "[VOĽBA...] <názovKonta>"

#: passwd.c:190
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: nesprávny argument %s: %s\n"

#: passwd.c:248
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s: Iba jedno z -l, -u, -d, -S môže byť zadané.\n"

#: passwd.c:257
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr "%s: Nemožno použiť jedno z -l, -u, -d, -S spolu s jedným z -i, -n, -w, -x.\n"

#: passwd.c:281
#, c-format
msgid "Only root can do that.\n"
msgstr "Toto môže vykonať iba správca.\n"

#: passwd.c:294
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: Meno užívateľa smie zadať iba správca.\n"

#: passwd.c:303
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: Zadané meno užívateľa je príliš dlhé.\n"

#: passwd.c:313
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: Môže byť zadané iba jedno meno užívateľa.\n"

#: passwd.c:323
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: Táto voľba vyžaduje meno užívateľa.\n"

#: passwd.c:334
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: Nemožno vás identifikovať!\n"

#: passwd.c:343
#: passwd.c:378
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: Neznáme meno užívateľa '%s'.\n"

#: passwd.c:395
#, c-format
msgid "%s: %s is not authorized to change the password of %s\n"
msgstr "%s: %s nie je autorizovaný pre zmenu hesla %s\n"

#: passwd.c:408
#, c-format
msgid "Locking password for user %s.\n"
msgstr "Zamykanie hesla pre užívateľa %s.\n"

#: passwd.c:412
#: passwd.c:424
#: passwd.c:438
#: passwd.c:458
msgid "Success"
msgstr "Úspešné"

#: passwd.c:412
#: passwd.c:427
msgid "Error (password not set?)"
msgstr "Chyba (heslo nie je nastavene?)"

#: passwd.c:420
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "Odomykanie hesla pre užívateľa %s.\n"

#: passwd.c:426
msgid "Unsafe operation (use -f to force)"
msgstr "Nebezpečná operácia (použite -f pre vynútenie)"

#: passwd.c:435
#, c-format
msgid "Removing password for user %s.\n"
msgstr "Odstránenie hesla pre užívateľa %s.\n"

#: passwd.c:438
#: passwd.c:458
msgid "Error"
msgstr "Chyba"

#: passwd.c:455
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "Úprava dát stárnutia hesla pre užívateľa %s.\n"

#: passwd.c:471
#, c-format
msgid "Changing password for user %s.\n"
msgstr "Zmena hesla pre užívateľa %s.\n"

#: passwd.c:483
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: chyba pri čítaní zo vstupu: %s\n"

#: passwd.c:500
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: nepodarilo sa spustiť pam: %s\n"

#: passwd.c:513
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: nepodarilo sa nastaviť tty pre pam: %s\n"

#: passwd.c:524
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: nepodarilo sa nastaviť čakanie po zlyhaní: %s\n"

#: passwd.c:542
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s: vypršané autentifikačné kľúče úspešne aktualizované.\n"

#: passwd.c:548
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: všetky autentifikačné kľúče úspešne aktualizované.\n"

