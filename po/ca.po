#: libuser.c:88
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: error d'inicialització del libuser:"

#: libuser.c:165
msgid "Warning: unlocked password would be empty."
msgstr "Avís: la contrasenya desblocada seria buida."

#: libuser.c:279
msgid "Corrupted passwd entry."
msgstr "L'entrada de la contrasenya està malmesa."

#: libuser.c:296
msgid "Empty password."
msgstr "La contrasenya és buida."

#: libuser.c:299
msgid "Password locked."
msgstr "La contrasenya està blocada."

#: libuser.c:303
msgid "Password set, MD5 crypt."
msgstr "La contrasenya està establerta, i xifrada amb MD5."

#: libuser.c:306
msgid "Password set, blowfish crypt."
msgstr "La contrasenya està establerta, i xifrada amb blowfish."

#: libuser.c:309
msgid "Password set, SHA256 crypt."
msgstr "La contrasenya està establerta, i xifrada amb SHA256."

#: libuser.c:312
msgid "Password set, SHA512 crypt."
msgstr "La contrasenya està establerta, i xifrada amb SHA512."

#: libuser.c:314
msgid "Password set, unknown crypt variant."
msgstr "La contrasenya està establerta, i xifrada amb una variant desconeguda."

#: libuser.c:317
msgid "Alternate authentication scheme in use."
msgstr "S'està emprant un esquema d'autenticació alternatiu."

#: libuser.c:322
msgid "Password set, DES crypt."
msgstr "La contrasenya està establerta, i xifrada amb DES."

#: libuser.c:336
#, c-format
msgid "No password set.\n"
msgstr "No hi ha cap contrasenya establerta.\n"

#: libuser.c:340
#, c-format
msgid "Unknown user.\n"
msgstr "No es coneix l'usuari.\n"

#: libuser.c:435
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s: el compte d'usuari no permet que la contrasenya envelleixi.\n"

#: passwd.c:158
msgid "keep non-expired authentication tokens"
msgstr "mantén testimonis d'autenticació que no hagin vençut"

#: passwd.c:160
msgid "delete the password for the named account (root only)"
msgstr "suprimeix la contrasenya per al compte indicat (només el superusuari)"

#: passwd.c:162
msgid "lock the password for the named account (root only)"
msgstr ""

#: passwd.c:164
msgid "unlock the password for the named account (root only)"
msgstr ""

#: passwd.c:166
msgid "expire the password for the named account (root only)"
msgstr ""

#: passwd.c:168
msgid "force operation"
msgstr "força l'operació"

#: passwd.c:170
msgid "maximum password lifetime (root only)"
msgstr "temps de vida màxim de la contrasenya (només el superusuari)"

#: passwd.c:172
msgid "minimum password lifetime (root only)"
msgstr "temps de vida mínim de la contrasenya (només el superusuari)"

#: passwd.c:174
msgid ""
"number of days warning users receives before password expiration (root only)"
msgstr ""
"el nombre de dies abans que venci la contrasenya perquè l'usuari en rebi "
"l'avís (només el superusuari)"

#: passwd.c:177
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr ""
"el nombre de dies després que venci la contrasenya perquè el compte de "
"l'usuari venci (només el superusuari)"

#: passwd.c:180
msgid "report password status on the named account (root only)"
msgstr ""
"informa de l'estat de la contrasenya del compte indicat (només el "
"superusuari)"

#: passwd.c:182
msgid "read new tokens from stdin (root only)"
msgstr "llegeix nous testimonis de l'entrada estàndard (només el superusuari)"

#: passwd.c:189
msgid "[OPTION...] <accountName>"
msgstr "[OPCIÓ...] <NomDelCompte>"

#: passwd.c:192
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: argument %s equivocat: %s\n"

#: passwd.c:253
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr ""
"%s: només es pot especificar un d'aquests senyaladors al mateix temps:  -l, -"
"u, -d, -S.\n"

#: passwd.c:262
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr ""
"%s: no es poden barrejar -l, -u, -d, -S amb cap d'aquest: -i, -n, -w, -x.\n"

#: passwd.c:281
#, c-format
msgid "Only root can do that.\n"
msgstr "Només el superusuari pot fer-ho.\n"

#: passwd.c:294
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: només el superusuari pot especificar un nom d'usuari.\n"

#: passwd.c:303
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: el nom d'usuari indicat és massa llarg.\n"

#: passwd.c:313
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: només es pot indicar un sol nom d'usuari.\n"

#: passwd.c:323
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: aquesta operació requereix un nom d'usuari.\n"

#: passwd.c:334
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: no se us pot identificar.\n"

#: passwd.c:343 passwd.c:378
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: no es coneix el nom d'usuari «%s».\n"

#: passwd.c:387
#, c-format
msgid "%s: SELinux denying access due to security policy.\n"
msgstr ""

#: passwd.c:397
#, c-format
msgid "Locking password for user %s.\n"
msgstr "S'està blocant la contrasenya de l'usuari %s.\n"

#: passwd.c:401 passwd.c:413 passwd.c:428 passwd.c:439 passwd.c:459
msgid "Success"
msgstr "Correcte"

#: passwd.c:401 passwd.c:416
msgid "Error (password not set?)"
msgstr "Error (no s'ha establert la contrasenya, potser?)"

#: passwd.c:409
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "S'està desblocant la contrasenya de l'usuari %s.\n"

#: passwd.c:415
msgid "Unsafe operation (use -f to force)"
msgstr "L'operació no és segura (empreu -f per a forçar)"

#: passwd.c:424
#, c-format
msgid "Expiring password for user %s.\n"
msgstr ""

#: passwd.c:428 passwd.c:439 passwd.c:459
msgid "Error"
msgstr "Error"

#: passwd.c:436
#, c-format
msgid "Removing password for user %s.\n"
msgstr "S'està suprimint la contrasenya de l'usuari %s.\n"

#: passwd.c:456
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "S'està ajustant la data d'envelliment de l'usuari %s.\n"

#: passwd.c:472
#, c-format
msgid "Changing password for user %s.\n"
msgstr "S'està canviant la contrasenya de l'usuari %s.\n"

#: passwd.c:484
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: s'ha produït un error en llegir de l'entarda estàndard: %s\n"

#: passwd.c:501
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: no s'ha pogut iniciar el pam: %s\n"

#: passwd.c:514
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: no s'ha ogut establir la tty per al pam: %s\n"

#: passwd.c:525
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: no s'ha pogut establir l'endarreriment per a l'error: %s\n"

#: passwd.c:543
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s: els testimonis d'autenticació s'han actualitzat amb èxit.\n"

#: passwd.c:549
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: tots els testimonis d'autenticació s'han actualitzat amb èxit.\n"
