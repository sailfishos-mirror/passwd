#: libuser.c:90
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: libuser प्रारंभीकरणेवेळी त्रूटी:"

#: libuser.c:167
msgid "Warning: unlocked password would be empty."
msgstr "सावधान: बिनकुलुपी गुप्तशब्ध रीक्त असेल."

#: libuser.c:281
msgid "Corrupted passwd entry."
msgstr "passwd प्रविष्ट कुरचीत."

#: libuser.c:298
msgid "Empty password."
msgstr "गुप्तशब्द रीक्त आहे."

#: libuser.c:301
msgid "Password locked."
msgstr "गुप्तशब्द कुलुपबंद."

#: libuser.c:305
msgid "Password set, MD5 crypt."
msgstr "गुप्तशब्द निश्चित, MD5 क्रीप्ट."

#: libuser.c:308
msgid "Password set, blowfish crypt."
msgstr "गुप्तशब्द निश्चित,blowfish क्रीप्ट."

#: libuser.c:310
msgid "Password set, unknown crypt variant."
msgstr "गुप्तशब्द निश्चित,अपरिचीत क्रीप्ट विकल्प."

#: libuser.c:313
msgid "Alternate authentication scheme in use."
msgstr "अगाऊ अधिप्रमाणन रचना वापरणीत आहे."

#: libuser.c:318
msgid "Password set, DES crypt."
msgstr "गुप्तशब्द निश्चित, DES क्रीप्ट."

#: libuser.c:332
#, c-format
msgid "No password set.\n"
msgstr "गुप्तशब्द निश्चित नाही.\n"

#: libuser.c:336
#, c-format
msgid "Unknown user.\n"
msgstr "अपरिचीत वापर्कता.\n"

#: libuser.c:431
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s: जुना होत असलेल्या गुप्तशब्दास वापर्कता खातेचे समर्थन नाही.\n"

#: passwd.c:158
msgid "keep non-expired authentication tokens"
msgstr "मुदत समाप्ती-नसणाऱ्या अधिप्रमाणन टोकन ठेवा"

#: passwd.c:160
msgid "delete the password for the named account (root only)"
msgstr "नामांकीत खातेंकरीता गुप्तशब्द काढून टाका (फक्त रूट)"

#: passwd.c:162
msgid "lock the named account (root only)"
msgstr "नामांकीत खाती कुलूपबंद करा (फक्त रूट)"

#: passwd.c:164
msgid "unlock the named account (root only)"
msgstr "नामांकीत खाती कुलूपबंद करू नका (फक्त रूट)"

#: passwd.c:166
msgid "force operation"
msgstr "जबरदस्तीने कार्यरत करा"

#: passwd.c:168
msgid "maximum password lifetime (root only)"
msgstr "कमाल गुप्तशब्द कार्यकाळ (फक्त रूट)"

#: passwd.c:170
msgid "minimum password lifetime (root only)"
msgstr "कीमान गुप्तशब्द कार्यकाळ (फक्त रूट)"

#: passwd.c:172
msgid "number of days warning users receives before password expiration (root only)"
msgstr "गुप्तशब्द मुदत समाप्तीच्या पहिले वापरकर्त्यांना काहिक दिवसांची सावधानता मिळते (फक्त रूट)"

#: passwd.c:175
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr "गुप्तशब्द मुदत समाप्तीवर खाते अकार्यान्वित झाल्यावरचे दिवस (फक्त रूट)"

#: passwd.c:178
msgid "report password status on the named account (root only)"
msgstr "नामांकीत खात्यांवर गुप्तशब्द स्तिथीचा अहवाल द्यावे (फक्त रूट)"

#: passwd.c:180
msgid "read new tokens from stdin (root only)"
msgstr "stdin पासून नविन टोकन वाचा (फक्त रूट)"

#: passwd.c:187
msgid "[OPTION...] <accountName>"
msgstr "[OPTION...] <accountName>"

#: passwd.c:190
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: वाईट गुणधर्म %s: %s\n"

#: passwd.c:248
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s: फक्त -l, -u, -d, -S पैकी एकच निश्चित केल्या जाऊ शकते.\n"

#: passwd.c:257
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr "%s: -l, -u, -d, -S पैकी व -i, -n, -w, -x पैकी एक मिश्र करू शकत नाही.\n"

#: passwd.c:281
#, c-format
msgid "Only root can do that.\n"
msgstr "फक्त रूटच हे करू शकतो.\n"

#: passwd.c:294
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: फक्त रूटच वापरकर्त्याचे नाव निश्चित करू शकतो.\n"

#: passwd.c:303
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: पुरविलेल्या वापरकर्त्याचे नाव खूपच लांब आहे.\n"

#: passwd.c:313
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: फक्त एकच वापर कर्त्याचे नाव निश्चित केल्या जाऊ शकतो.\n"

#: passwd.c:323
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: या पर्यायास वापरकर्तेच्या नावाची गरज आहे.\n"

#: passwd.c:334
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: आपल्याला ओळखले नाही!\n"

#: passwd.c:343 passwd.c:378
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: अपरीचीत वापरकर्त्याचे नाव '%s'.\n"

#: passwd.c:395
#, c-format
msgid "%s: %s is not authorized to change the password of %s\n"
msgstr "%s: %s चा गुप्तशब्द बदलिवण्यास %s अधिप्रमाणीत नाही\n"

#: passwd.c:408
#, c-format
msgid "Locking password for user %s.\n"
msgstr "%s वापरकर्त्यास गुप्तशब्द कुलूपबंद करत आहे.\n"

#: passwd.c:412 passwd.c:424 passwd.c:438 passwd.c:458
msgid "Success"
msgstr "यश"

#: passwd.c:412 passwd.c:427
msgid "Error (password not set?)"
msgstr "त्रूटी (गुप्तशब्द निश्चित नाही?)"

#: passwd.c:420
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "%s वापरकर्त्यासाठी गुप्तशब्द कुलूपबंदी नाही.\n"

#: passwd.c:426
msgid "Unsafe operation (use -f to force)"
msgstr "असुरक्षीत कार्यपद्धती (जबरदस्तीने -f चा वापर करा)"

#: passwd.c:435
#, c-format
msgid "Removing password for user %s.\n"
msgstr "%s वापरकर्त्यास गुप्तशब्द काढून टाका.\n"

#: passwd.c:438 passwd.c:458
msgid "Error"
msgstr "त्रूटी"

#: passwd.c:455
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "%s वापरकर्त्यास जुना डाटा जुळवून घेतो.\n"

#: passwd.c:471
#, c-format
msgid "Changing password for user %s.\n"
msgstr "%s वापरकर्त्यास गुप्तशब्द बदलत आहे.\n"

#: passwd.c:483
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s:stdin पासून वाचतेवेळी त्रूटी: %s\n"

#: passwd.c:500
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s:pam सुरू करण्यास असमर्थ: %s\n"

#: passwd.c:513
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: pam करीता tty निश्चित करण्यास असमर्थ: %s\n"

#: passwd.c:524
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: अयशस्वीरीत्या विलंब कार्यान्वीत करण्यास असमर्थ: %s\n"

#: passwd.c:542
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s: मुदत समाप्ती अधिप्रमाणीत टोकन यशस्वीरीत्या अद्ययावतीत केले.\n"

#: passwd.c:548
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: सर्व अधिप्रमाणीत टोकन यशस्वीरीत्या अद्ययावतीत केले.\n"

