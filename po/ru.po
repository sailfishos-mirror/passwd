#: libuser.c:88
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: ошибка инициализации libuser:"

#: libuser.c:165
msgid "Warning: unlocked password would be empty."
msgstr "Предупреждение: разблокированный пароль будет пустым."

#: libuser.c:279
msgid "Corrupted passwd entry."
msgstr "Поврежденная запись в passwd."

#: libuser.c:296
msgid "Empty password."
msgstr "Пустой пароль."

#: libuser.c:299
msgid "Password locked."
msgstr "Пароль заблокирован."

#: libuser.c:303
msgid "Password set, MD5 crypt."
msgstr "Пароль задан, шифр MD5."

#: libuser.c:306
msgid "Password set, blowfish crypt."
msgstr "Пароль задан, шифр blowfish."

#: libuser.c:309
msgid "Password set, SHA256 crypt."
msgstr "Пароль задан, шифр SHA256."

#: libuser.c:312
msgid "Password set, SHA512 crypt."
msgstr "Пароль задан, шифр SHA512."

#: libuser.c:314
msgid "Password set, unknown crypt variant."
msgstr "Пароль задан, шифр неизвестен."

#: libuser.c:317
msgid "Alternate authentication scheme in use."
msgstr "Используется альтернативная схема проверки подлинности."

#: libuser.c:322
msgid "Password set, DES crypt."
msgstr "Пароль задан, шифр DES."

#: libuser.c:336
#, c-format
msgid "No password set.\n"
msgstr "Пароль не задан.\n"

#: libuser.c:340
#, c-format
msgid "Unknown user.\n"
msgstr "Неизвестный пользователь.\n"

#: libuser.c:435
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s: учетная запись не поддерживает устаревание пароля.\n"

#: passwd.c:158
msgid "keep non-expired authentication tokens"
msgstr "сохранить неустаревшие токены проверки подлинности (пароли)"

#: passwd.c:160
msgid "delete the password for the named account (root only)"
msgstr "удалить пароль для учетной записи (разрешено только root)"

#: passwd.c:162
msgid "lock the password for the named account (root only)"
msgstr ""

#: passwd.c:164
msgid "unlock the password for the named account (root only)"
msgstr ""

#: passwd.c:166
msgid "expire the password for the named account (root only)"
msgstr ""

#: passwd.c:168
msgid "force operation"
msgstr "принудительное выполнение"

#: passwd.c:170
msgid "maximum password lifetime (root only)"
msgstr "максимальный срок действия пароля (разрешено только root)"

#: passwd.c:172
msgid "minimum password lifetime (root only)"
msgstr "минимальный срок действия пароля (разрешено только root)"

#: passwd.c:174
msgid ""
"number of days warning users receives before password expiration (root only)"
msgstr ""
"за сколько дней до даты устаревания пароля начать предупреждать пользователя "
"(разрешено только root)"

#: passwd.c:177
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr ""
"через сколько дней после устаревания пароля блокировать учетную запись "
"(разрешено только root)"

#: passwd.c:180
msgid "report password status on the named account (root only)"
msgstr "сообщить состояние пароля для учетной записи (разрешено только root)"

#: passwd.c:182
msgid "read new tokens from stdin (root only)"
msgstr "считать новое значение со стандартного входа (разрешено только root)"

#: passwd.c:189
msgid "[OPTION...] <accountName>"
msgstr "[ПАРАМЕТРЫ...] <учетная_запись>"

#: passwd.c:192
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: неверный аргумент %s: %s\n"

#: passwd.c:253
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr ""
"%s: Допускается указывать только один из параметров: -l, -u, -d или -S.\n"

#: passwd.c:262
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr "%s: Не допускается смешивать ключи -l, -u, -d, -S и -i, -n, -w, -x.\n"

#: passwd.c:281
#, c-format
msgid "Only root can do that.\n"
msgstr "Операция разрешена только пользователю root.\n"

#: passwd.c:294
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: Только root может указывать имя учетной записи.\n"

#: passwd.c:303
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: Указанное имя пользователя слишком длинное.\n"

#: passwd.c:313
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: Допускается указывать только одно имя.\n"

#: passwd.c:323
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: Для этого параметра необходимо имя пользователя.\n"

#: passwd.c:334
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: Не могу вас идентифицировать!\n"

#: passwd.c:343 passwd.c:378
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: Неизвестное имя '%s'.\n"

#: passwd.c:387
#, c-format
msgid "%s: SELinux denying access due to security policy.\n"
msgstr ""

#: passwd.c:397
#, c-format
msgid "Locking password for user %s.\n"
msgstr "Блокируется пароль для пользователя %s.\n"

#: passwd.c:401 passwd.c:413 passwd.c:428 passwd.c:439 passwd.c:459
msgid "Success"
msgstr "Успех"

#: passwd.c:401 passwd.c:416
msgid "Error (password not set?)"
msgstr "Ошибка (пароль не задан?)"

#: passwd.c:409
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "Снимается блокировка пароля для пользователя %s.\n"

#: passwd.c:415
msgid "Unsafe operation (use -f to force)"
msgstr "Операция не безопасна (используйте -f для принудительного выполнения)"

#: passwd.c:424
#, c-format
msgid "Expiring password for user %s.\n"
msgstr ""

#: passwd.c:428 passwd.c:439 passwd.c:459
msgid "Error"
msgstr "Ошибка"

#: passwd.c:436
#, c-format
msgid "Removing password for user %s.\n"
msgstr "Удаляется пароль для пользователя %s.\n"

#: passwd.c:456
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "Устанавливаются параметры устаревания для пользователя %s.\n"

#: passwd.c:472
#, c-format
msgid "Changing password for user %s.\n"
msgstr "Смена пароля для пользователя %s.\n"

#: passwd.c:484
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: ошибка чтения со стандартного входа: %s\n"

#: passwd.c:501
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: небезопасно запускать pam: %s\n"

#: passwd.c:514
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: не возможно задать tty для pam: %s\n"

#: passwd.c:525
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: невозможно задать задержку при сбое: %s\n"

#: passwd.c:543
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s: успешно обновлен устаревший токен проверки подлинности.\n"

#: passwd.c:549
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: все токены проверки подлинности успешно обновлены.\n"
