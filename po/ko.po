#: libuser.c:88
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: libuser 초기화 오류:"

#: libuser.c:165
msgid "Warning: unlocked password would be empty."
msgstr "경고: 잠금 해제로 비밀 번호 입력란이 공백으로 됩니다."

#: libuser.c:279
msgid "Corrupted passwd entry."
msgstr "손상된 passwd 항목"

#: libuser.c:296
msgid "Empty password."
msgstr "비밀 번호란이 비어있음."

#: libuser.c:299
msgid "Password locked."
msgstr "비밀 번호 잠금."

#: libuser.c:303
msgid "Password set, MD5 crypt."
msgstr "비밀번호 설정, MD5 암호화."

#: libuser.c:306
msgid "Password set, blowfish crypt."
msgstr "비밀번호 설정, blowfish 암호화"

#: libuser.c:309
msgid "Password set, SHA256 crypt."
msgstr "비밀번호 설정, SHA256 암호화. "

#: libuser.c:312
msgid "Password set, SHA512 crypt."
msgstr "비밀번호 설정, SHA512 암호화.  "

#: libuser.c:314
msgid "Password set, unknown crypt variant."
msgstr "비밀번호 설정, 알 수 없는 암호화 유형."

#: libuser.c:317
msgid "Alternate authentication scheme in use."
msgstr "다른 인증 스키마 사용 중"

#: libuser.c:322
msgid "Password set, DES crypt."
msgstr "비밀번호 설정, DES 암호화"

#: libuser.c:336
#, c-format
msgid "No password set.\n"
msgstr "비밀번호가 설정되지 않음.\n"

#: libuser.c:340
#, c-format
msgid "Unknown user.\n"
msgstr "알 수 없는 사용자.\n"

#: libuser.c:435
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s: 사용자 계정이 비밀번호 사용 기한을 지원하지 않음.\n"

#: passwd.c:158
msgid "keep non-expired authentication tokens"
msgstr "만기되지 않는 인증 토큰 사용"

#: passwd.c:160
msgid "delete the password for the named account (root only)"
msgstr "named 계정의 비밀번호 삭제 (root로만 실행 가능)"

#: passwd.c:162
msgid "lock the password for the named account (root only)"
msgstr "이름이 지정된 계정의 비밀 번호를 잠금 (root로만 실행 가능) "

#: passwd.c:164
msgid "unlock the password for the named account (root only)"
msgstr "이름이 지정된 계정의 비밀번호를 잠금 해제 (root로만 실행 가능)  "

#: passwd.c:166
msgid "expire the password for the named account (root only)"
msgstr "이름이 지정된 계정의 비밀 번호 만료 (root로만 실행 가능) "

#: passwd.c:168
msgid "force operation"
msgstr "강제 실행"

#: passwd.c:170
msgid "maximum password lifetime (root only)"
msgstr "비밀번호 최대 유효기간 (root로만 실행 가능)"

#: passwd.c:172
msgid "minimum password lifetime (root only)"
msgstr "비밀번호 최소 유효기간 (root로만 실행 가능)"

#: passwd.c:174
msgid ""
"number of days warning users receives before password expiration (root only)"
msgstr "비밀번호가 만료되기 전 사용자에게 주어지는 경고 기간 (root로만 실행 가능)"

#: passwd.c:177
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr "비밀번호 만료 후 계정이 비활성화되는 기간 (root로만 실행 가능)"

#: passwd.c:180
msgid "report password status on the named account (root only)"
msgstr "named 계정에 비밀 번호 상태 보고 (root로만 실행 가능)"

#: passwd.c:182
msgid "read new tokens from stdin (root only)"
msgstr "stdin에서 새 토큰을 읽음 (root로만 실행 가능)"

#: passwd.c:189
msgid "[OPTION...] <accountName>"
msgstr "[옵션...] <accountName>"

#: passwd.c:192
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: 잘못된 인수 %s: %s\n"

#: passwd.c:253
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s:  -l, -u, -d, -S 중 하나만을 지정할 수 있음.\n"

#: passwd.c:262
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr "%s: -l, -u, -d, -S 중 하나 또는 -i, -n, -w, -x 중 하나를 혼용할 수 없음.\n"

#: passwd.c:281
#, c-format
msgid "Only root can do that.\n"
msgstr "root로만 실행할 수 있습니다.\n"

#: passwd.c:294
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: root로만 사용자 이름을 지정할 수 있습니다.\n"

#: passwd.c:303
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: 사용자 이름이 너무 깁니다.\n"

#: passwd.c:313
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: 한 개의 사용자 이름만을 지정할 수 있습니다.\n"

#: passwd.c:323
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: 이 옵션에 사용자 이름이 필요합니다.\n"

#: passwd.c:334
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: 사용자를 확인할 수 없습니다!\n"

#: passwd.c:343 passwd.c:378
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: 알 수 없는 사용자 이름 '%s'.\n"

#: passwd.c:387
#, c-format
msgid "%s: SELinux denying access due to security policy.\n"
msgstr "%s: SELinux의 보안 정책에 따라 액세스를 거부합니다.\n"

#: passwd.c:397
#, c-format
msgid "Locking password for user %s.\n"
msgstr "%s 사용자의 비밀 번호 잠금\n"

#: passwd.c:401 passwd.c:413 passwd.c:428 passwd.c:439 passwd.c:459
msgid "Success"
msgstr "성공"

#: passwd.c:401 passwd.c:416
msgid "Error (password not set?)"
msgstr "오류 (비밀 번호가 설정되지 않음?)"

#: passwd.c:409
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "%s 사용자의 비밀 번호 잠금 해제 중\n"

#: passwd.c:415
msgid "Unsafe operation (use -f to force)"
msgstr "불안전 작동 (-f를 사용하여 강제함)"

#: passwd.c:424
#, c-format
msgid "Expiring password for user %s.\n"
msgstr "사용자 %s의 비밀 번호가 만료됨.\n"

#: passwd.c:428 passwd.c:439 passwd.c:459
msgid "Error"
msgstr "오류"

#: passwd.c:436
#, c-format
msgid "Removing password for user %s.\n"
msgstr "%s 사용자의 비밀 번호 삭제 중\n"

#: passwd.c:456
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "%s 사용자의 사용 기한 데이터 조정 중\n"

#: passwd.c:472
#, c-format
msgid "Changing password for user %s.\n"
msgstr "%s 사용자의 비밀 번호 변경 중\n"

#: passwd.c:484
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: stdin에서 읽기 오류: %s\n"

#: passwd.c:501
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: pam 시작 불가능: %s\n"

#: passwd.c:514
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: pam의 tty 설정 불가능: %s\n"

#: passwd.c:525
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: 실패 지연 설정 불가능: %s\n"

#: passwd.c:543
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s: 만기된 인증 토큰이 성공적으로 업데이트 되었습니다.\n"

#: passwd.c:549
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: 모든 인증 토큰이 성공적으로 업데이트 되었습니다.\n"
