#: libuser.c:90
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: грешка при иницијализацији libuser-а:"

#: libuser.c:167
msgid "Warning: unlocked password would be empty."
msgstr "Упозорење: откључана лозинка ће бити празна."

#: libuser.c:281
msgid "Corrupted passwd entry."
msgstr "Оштећен унос лозинке."

#: libuser.c:298
msgid "Empty password."
msgstr "Празна лозинка."

#: libuser.c:301
msgid "Password locked."
msgstr "Лозинка закључана."

#: libuser.c:305
msgid "Password set, MD5 crypt."
msgstr "Лозинка је постављена, шифрирање MD5."

#: libuser.c:308
msgid "Password set, blowfish crypt."
msgstr "Лозинка је постављена, шифрирање Blowfish."

#: libuser.c:310
msgid "Password set, unknown crypt variant."
msgstr "Лозинка је постављена, врста шифрирања није позната."

#: libuser.c:313
msgid "Alternate authentication scheme in use."
msgstr "Алтернативна шема за аутентификацију се користи."

#: libuser.c:318
msgid "Password set, DES crypt."
msgstr "Лозинка је постављена, шифрирање DES."

#: libuser.c:332
#, c-format
msgid "No password set.\n"
msgstr "Лозинка није постављена.\n"

#: libuser.c:336
#, c-format
msgid "Unknown user.\n"
msgstr "Непознати корисник.\n"

#: libuser.c:431
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s: кориснички налог нема подршку за застаривање важења лозинке.\n"

#: passwd.c:158
msgid "keep non-expired authentication tokens"
msgstr "задржи изразе за аутентификацију који нису истекли"

#: passwd.c:160
msgid "delete the password for the named account (root only)"
msgstr "избриши лозинку за именовани налог (само root)"

#: passwd.c:162
msgid "lock the named account (root only)"
msgstr "закључај само именовани налог (само root)"

#: passwd.c:164
msgid "unlock the named account (root only)"
msgstr "откључај именовани налог (само root)"

#: passwd.c:166
msgid "force operation"
msgstr "Присили операцију"

#: passwd.c:168
msgid "maximum password lifetime (root only)"
msgstr "Најдужи рок трајања лозинке (само root)"

#: passwd.c:170
msgid "minimum password lifetime (root only)"
msgstr "Најкраћи рок трајања лозинке (само root)"

#: passwd.c:172
msgid "number of days warning users receives before password expiration (root only)"
msgstr "број дана током којих је корисник упозорен пре истека лозинке (само root)"

#: passwd.c:175
msgid "number of days after password expiration when an account becomes disabled (root only)"
msgstr "број дана после истека лозинке када налог постаје онемогућен (само root)"

#: passwd.c:178
msgid "report password status on the named account (root only)"
msgstr "извести о стању лозинке именованог налога (само root)"

#: passwd.c:180
msgid "read new tokens from stdin (root only)"
msgstr "читање нових израза са stdin (само root)"

#: passwd.c:187
msgid "[OPTION...] <accountName>"
msgstr "[ОПЦИЈА...] <имеНалога>"

#: passwd.c:190
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: лоша аргумент %s: %s\n"

#: passwd.c:248
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s: Само једно од -l, -u, -d, -S може бити одређено.\n"

#: passwd.c:257
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr "%s: један од аргумената -l, -u, -d, -S се неможе мешати са једним од аргумената -i, -n, -w, -x.\n"

#: passwd.c:281
#, c-format
msgid "Only root can do that.\n"
msgstr "Само root може то да уради.\n"

#: passwd.c:294
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s:Само root може одредити корисничко име.\n"

#: passwd.c:303
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: Унесено корисничко име је предуго.\n"

#: passwd.c:313
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: Само једно корисничко име може бити одређено.\n"

#: passwd.c:323
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: Ова опција захтева корисничко име.\n"

#: passwd.c:334
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: Није вас могуће препознати!\n"

#: passwd.c:343
#: passwd.c:378
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: Непознато корисничко име '%s'.\n"

#: passwd.c:395
#, c-format
msgid "%s: %s is not authorized to change the password of %s\n"
msgstr "%s: %s није овлашћен да промени лозинку за %s\n"

#: passwd.c:408
#, c-format
msgid "Locking password for user %s.\n"
msgstr "Закључавање лозинке корисника %s.\n"

#: passwd.c:412
#: passwd.c:424
#: passwd.c:438
#: passwd.c:458
msgid "Success"
msgstr "Успех"

#: passwd.c:412
#: passwd.c:427
msgid "Error (password not set?)"
msgstr "Грешка (можда лозинка није постављена?)"

#: passwd.c:420
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "Откључавање лозинке корисника %s.\n"

#: passwd.c:426
msgid "Unsafe operation (use -f to force)"
msgstr "Несигурна операција (користи -f да се присили)"

#: passwd.c:435
#, c-format
msgid "Removing password for user %s.\n"
msgstr "Уклањање лозинке корисника %s.\n"

#: passwd.c:438
#: passwd.c:458
msgid "Error"
msgstr "Грешка"

#: passwd.c:455
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "Прилагођавање података о застаревању за корисника %s.\n"

#: passwd.c:471
#, c-format
msgid "Changing password for user %s.\n"
msgstr "Измена лозинке за корисника %s.\n"

#: passwd.c:483
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: грешка при читању stdin: %s\n"

#: passwd.c:500
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: није могуће покренути pam: %s\n"

#: passwd.c:513
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: није могуће поставити tty за pam: %s\n"

#: passwd.c:524
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: није могуће поставити одлагање неуспеха : %s\n"

#: passwd.c:542
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s: изрази за аутентификацију који су истекли су успешно ажурирани.\n"

#: passwd.c:548
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: сви изрази за аутентификацију су успешно ажурирани.\n"

