#: libuser.c:88
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: greška pri libuser inicijalizaciji:"

#: libuser.c:165
msgid "Warning: unlocked password would be empty."
msgstr "Upozorenje: otključana lozinka će biti prazna."

#: libuser.c:279
msgid "Corrupted passwd entry."
msgstr "Oštećena stavka lozinke."

#: libuser.c:296
msgid "Empty password."
msgstr "Prazna lozinka."

#: libuser.c:299
msgid "Password locked."
msgstr "Lozinka zaključana."

#: libuser.c:303
msgid "Password set, MD5 crypt."
msgstr "Lozinka je postavljena, MD5 šifriranje."

#: libuser.c:306
msgid "Password set, blowfish crypt."
msgstr "Lozinka je postavljena, Blowfish šifriranje."

#: libuser.c:309
msgid "Password set, SHA256 crypt."
msgstr "Lozinka je postavljena, SHA256 šifriranje."

#: libuser.c:312
msgid "Password set, SHA512 crypt."
msgstr "Lozinka je postavljena, SHA512 šifriranje."

#: libuser.c:314
msgid "Password set, unknown crypt variant."
msgstr "Lozinka je postavljena, vrsta šifriranja nije poznata."

#: libuser.c:317
msgid "Alternate authentication scheme in use."
msgstr "Alternativna šema za autentifikaciju se koristi."

#: libuser.c:322
msgid "Password set, DES crypt."
msgstr "Lozinka je postavljena, DES šifriranje."

#: libuser.c:336
#, c-format
msgid "No password set.\n"
msgstr "Lozinka nije postavljena.\n"

#: libuser.c:340
#, c-format
msgid "Unknown user.\n"
msgstr "Nepoznati korisnik.\n"

#: libuser.c:435
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s: korisnički nalog nema podršku za zastarevanje važenja lozinke.\n"

#: passwd.c:158
msgid "keep non-expired authentication tokens"
msgstr "zadrži izraze za autentifikaciju koji nisu istekli"

#: passwd.c:160
msgid "delete the password for the named account (root only)"
msgstr "izbriši lozinku za imenovani nalog (samo root)"

#: passwd.c:162
msgid "lock the password for the named account (root only)"
msgstr ""

#: passwd.c:164
msgid "unlock the password for the named account (root only)"
msgstr ""

#: passwd.c:166
msgid "expire the password for the named account (root only)"
msgstr ""

#: passwd.c:168
msgid "force operation"
msgstr "prisili operaciju"

#: passwd.c:170
msgid "maximum password lifetime (root only)"
msgstr "najduži rok trajanja lozinke (samo root)"

#: passwd.c:172
msgid "minimum password lifetime (root only)"
msgstr "najkraći rok trajanja lozinke (samo root)"

#: passwd.c:174
msgid ""
"number of days warning users receives before password expiration (root only)"
msgstr "broj dana tokom kojih korisnik prima upozorenja pre isteka lozinke (samo root)"

#: passwd.c:177
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr "broj dana posle isteka lozinke kada nalog postaje isključen (samo root)"

#: passwd.c:180
msgid "report password status on the named account (root only)"
msgstr "izvesti o stanju lozinke imenovanog naloga (samo root)"

#: passwd.c:182
msgid "read new tokens from stdin (root only)"
msgstr "čitanje novih izraza sa stdin (samo root)"

#: passwd.c:189
msgid "[OPTION...] <accountName>"
msgstr "[OPCIJA...] <imeNaloga>"

#: passwd.c:192
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: loš argument %s: %s\n"

#: passwd.c:253
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s: Samo jedna od -l, -u, -d, -S opcija sme biti navedena.\n"

#: passwd.c:262
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr "%s: Ne mogu se mešati jedna od -l, -u, -d, -S opcija sa jednom od -i, -n, -w, -x.\n"

#: passwd.c:281
#, c-format
msgid "Only root can do that.\n"
msgstr "Samo root može to da uradi.\n"

#: passwd.c:294
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: Samo root može navesti korisničko ime.\n"

#: passwd.c:303
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: Navedeno korisničko ime je predugo.\n"

#: passwd.c:313
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: Samo jedno korisničko ime može biti navedeno.\n"

#: passwd.c:323
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: Ova opcija zahteva korisničko ime.\n"

#: passwd.c:334
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: Nije vas moguće prepoznati!\n"

#: passwd.c:343 passwd.c:378
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: Nepoznato korisničko ime „%s“.\n"

#: passwd.c:387
#, c-format
msgid "%s: SELinux denying access due to security policy.\n"
msgstr ""

#: passwd.c:397
#, c-format
msgid "Locking password for user %s.\n"
msgstr "Zaključavanje lozinke za korisnika %s.\n"

#: passwd.c:401 passwd.c:413 passwd.c:428 passwd.c:439 passwd.c:459
msgid "Success"
msgstr "Uspeh"

#: passwd.c:401 passwd.c:416
msgid "Error (password not set?)"
msgstr "Greška (lozinka nije postavljena?)"

#: passwd.c:409
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "Otključavanje lozinke za korisnika %s.\n"

#: passwd.c:415
msgid "Unsafe operation (use -f to force)"
msgstr "Nesigurna operacija (koristite -f za prisiljavanje)"

#: passwd.c:424
#, c-format
msgid "Expiring password for user %s.\n"
msgstr ""

#: passwd.c:428 passwd.c:439 passwd.c:459
msgid "Error"
msgstr "Greška"

#: passwd.c:436
#, c-format
msgid "Removing password for user %s.\n"
msgstr "Uklanjanje lozinke za korisnika %s.\n"

#: passwd.c:456
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "Prilagođavanje podataka o zastarevanju za korisnika %s.\n"

#: passwd.c:472
#, c-format
msgid "Changing password for user %s.\n"
msgstr "Izmena lozinke za korisnika %s.\n"

#: passwd.c:484
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: greška pri čitanju sa stdin: %s\n"

#: passwd.c:501
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: nije moguće pokrenuti pam: %s\n"

#: passwd.c:514
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: nije moguće postaviti tty za pam: %s\n"

#: passwd.c:525
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: nije moguće postaviti odlaganje neuspeha: %s\n"

#: passwd.c:543
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s: izrazi za autentifikaciju koji su istekli su uspešno ažurirani.\n"

#: passwd.c:549
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: svi izrazi za autentifikaciju su uspešno ažurirani.\n"
