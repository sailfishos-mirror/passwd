#: libuser.c:298
msgid "Empty password."
msgstr "ਖਾਲੀ ਗੁਪਤ-ਕੋਡ"

#: libuser.c:301
msgid "Password locked."
msgstr "ਗੁਪਤ-ਕੋਡ ਤਾਲਾਬੰਦ"

#: libuser.c:305
msgid "Password set, MD5 crypt."
msgstr "ਗੁਪਤ-ਕੋਡ ਦਿੱਤਾ, MD5 ਇੰਕ੍ਰਿਪਟ"

#: libuser.c:308
