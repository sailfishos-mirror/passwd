#: libuser.c:91
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: libuser ਸ਼ੁਰੂਆਤ ਗਲਤੀ:"

#: libuser.c:157
msgid "Warning: unlocked password would be empty."
msgstr "ਚੇਤਾਵਨੀ: ਖੋਲ੍ਹਣ ਪਾਸਵਰਡ ਖਾਲੀ ਹੋਵੇਗਾ।"

#: libuser.c:203
msgid "Note: deleting a password also unlocks the password."
msgstr "ਨੋਟ: ਪਾਸਵਰਡ ਹਟਾਉਣ ਨਾਲ ਪਾਸਵਰਡ ਅਣ-ਲਾਕ ਵੀ ਹੁੰਦਾ ਹੈ।"

#: libuser.c:267
msgid "Corrupted passwd entry."
msgstr "ਨਿਕਾਰਾ ਪਾਸਵਰਡ ਐਂਟਰੀ ਹੈ।"

#: libuser.c:284
msgid "Empty password."
msgstr "ਖਾਲੀ ਪਾਸਵਰਡ"

#: libuser.c:287
msgid "Password locked."
msgstr "ਪਾਸਵਰਡ ਲਾਕ ਹੈ।"

#: libuser.c:291
msgid "Password set, MD5 crypt."
msgstr "ਪਾਸਵਰਡ ਦਿੱਤਾ, MD੫ ਇੰਕ੍ਰਿਪਟ"

#: libuser.c:294
msgid "Password set, blowfish crypt."
msgstr "ਪਾਸਵਰਡ ਦਿੱਤਾ, blowfish ਇੰਕ੍ਰਿਪਟ"

#: libuser.c:297
msgid "Password set, SHA256 crypt."
msgstr "ਪਾਸਵਰਡ ਸੈੱਟ, SHA੨੫੬ ਕ੍ਰਿਪਟ।"

#: libuser.c:300
msgid "Password set, SHA512 crypt."
msgstr "ਪਾਸਵਰਡ ਸੈੱਟ, SHA੫੧੨ ਕ੍ਰਿਪਟ।"

#: libuser.c:302
msgid "Password set, unknown crypt variant."
msgstr "ਪਾਸਵਰਡ ਦਿੱਤਾ, ਅਣਜਾਣ ਇੰਕ੍ਰਿਪਟ ਗੁਣ ਹੈ।"

#: libuser.c:305
msgid "Alternate authentication scheme in use."
msgstr "ਬਦਲਵਾਂ ਪਰਮਾਣਕਿਤਾ ਸਕੀਮ ਵਰਤੀ ਗਈ ਹੈ।"

#: libuser.c:310
msgid "Password set, DES crypt."
msgstr "ਪਾਸਵਰਡ ਦਿੱਤਾ DES ਦਿੱਤਾ"

#: libuser.c:323
#, c-format
msgid "No password set.\n"
msgstr "ਕੋਈ ਪਾਸਵਰਡ ਨਹੀਂ ਦਿੱਤਾ।\n"

#: libuser.c:328
#, c-format
msgid "Unknown user.\n"
msgstr "ਅਣਪਛਾਤਾ ਵਰਤੋਂਕਾਰ।"

#: libuser.c:403
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s ਵਰਤੋਂਕਾਰ ਅਕਾਊਂਟ ਪਾਸਵਰਡ ਉਮਰ ਨਾਲ ਸਹਿਯੋਗ ਨਹੀਂ ਹੈ।\n"

#: passwd.c:158
msgid "keep non-expired authentication tokens"
msgstr "ਨਾ-ਮਿਆਦ ਪੱਗੇ ਪਰਮਾਣਿਕਤਾ ਟੋਕਣ ਰੱਖੋ"

#: passwd.c:160
msgid ""
"delete the password for the named account (root only); also removes password "
"lock if any"
msgstr ""
"ਖਾਸ ਖਾਤੇ (ਸਿਰਫ਼ ਰੂਟ) ਲਈ ਪਾਸਵਰਡ ਹਟਾਓ; ਜੇ ਕੋਈ ਪਾਸਵਰਡ ਲਾਕ ਹੋਵੇ ਤਾਂ ਉਹ ਵੀ ਹਟਾਓ"

#: passwd.c:163
msgid "lock the password for the named account (root only)"
msgstr "ਖਾਸ ਖਾਤੇ (ਸਿਰਫ਼ ਰੂਟ) ਲਈ ਪਾਸਵਰਡ ਲਾਕ ਕਰੋ"

#: passwd.c:166
msgid "unlock the password for the named account (root only)"
msgstr "ਨਾਮੀ ਅਕਾਊਂਟ ਲਈ ਪਾਸਵਰਡ ਅਣਲਾਕ ਕਰੋ (root ਲਈ)"

#: passwd.c:169
msgid "expire the password for the named account (root only)"
msgstr "ਖਾਸ ਖਾਤੇ (ਸਿਰਫ਼ ਰੂਟ) ਲਈ ਪਾਸਵਰਡ ਦੀ ਮਿਆਦ ਪੁੱਗੀ"

#: passwd.c:172
msgid "force operation"
msgstr "ਧੱਕੇ ਨਾਲ"

#: passwd.c:174
msgid "maximum password lifetime (root only)"
msgstr "ਵੱਧ ਤੋਂ ਵੱਧ ਪਾਸਵਰਡ ਉਮਰ (root ਹੀ)"

#: passwd.c:176
msgid "minimum password lifetime (root only)"
msgstr "ਘੱਟੋ-ਘੱਟ ਪਾਸਵਰਡ ਉਮਰ  (root ਹੀ)"

#: passwd.c:178
msgid ""
"number of days warning users receives before password expiration (root only)"
msgstr ""
"ਪਾਸਵਰਡ ਦੀ ਮਿਆਦ ਪੁੱਗਣ (root ਲਈ) ਤੋਂ ਪਹਿਲਾਂ ਵਰਤੋਂਕਾਰਾਂ ਨੂੰ ਚੇਤਾਵਨੀ ਦੇਣ ਲਈ ਦਿਨ"

#: passwd.c:181
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr "ਅਕਾਊਂਟ ਬੰਦ ਕਰਨ (ਸਿਰਫ਼ root) ਲਈ ਮਿਆਦ ਪੁੱਗਣ ਬਾਰੇ ਦਿਨ ਬਾਰੇ ਦੱਸ ਜੀ"

#: passwd.c:184
msgid "report password status on the named account (root only)"
msgstr "ਨਾਮੀ ਅਕਾਊਂਟ (root ਲਈ) ਲਈ ਪਾਸਵਰਡ ਹਾਲਤ ਰਿਪੋਰਟ ਕਰੋ"

#: passwd.c:187
msgid "read new tokens from stdin (root only)"
msgstr "stdin (root only) ਤੋਂ ਨਵੇਂ ਟੋਕਨ ਪੜ੍ਹੋ"

#: passwd.c:194
msgid "[OPTION...] <accountName>"
msgstr "[OPTION...] <accountName>"

#: passwd.c:197
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: ਗਲਤ ਮੁੱਲ %s: %s\n"

#: passwd.c:258
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s: -l, -u, -d, -S ਵਿੱਚੋਂ ਕੇਵਲ ਇੱਕ ਹੀ ਦਿੱਤਾ ਜਾ ਸਕਦਾ ਹੈ।\n"

#: passwd.c:267
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr ""
"%s:  -l, -u, -d, -S ਵਿੱਚੋਂ ਕੋਈ ਇੱਕ ਤੇ -i, -n, -w, -x ਵਿੱਚੋਂ ਕਿਸੇ ਇੱਕ ਨੂੰ ਮਿਲਾ"
"ਇਆ ਨਹੀਂ ਜਾ ਸਕਦਾ\n"

#: passwd.c:286
#, c-format
msgid "Only root can do that.\n"
msgstr "ਸਿਰਫ root ਹੀ ਕਰ ਸਕਦਾ ਹੈ।\n"

#: passwd.c:299
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: root ਹੀ ਵਰਤੋਂਕਾਰ ਨਾਂ ਦੇ ਸਕਦਾ ਹੈ।\n"

#: passwd.c:308
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: ਵਰਤੋਂਕਾਰ ਨਾਂ ਬਹੁਤ ਲੰਮਾ ਹੈ।\n"

#: passwd.c:318
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: ਸਿਰਫ਼ ਵਰਤੋਂਕਾਰ ਦਿੱਤਾ ਜਾ ਸਕਦਾ ਹੈ।\n"

#: passwd.c:328
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: ਵਰਤੋਂਕਾਰ ਨਾਂ ਨਾਲ ਇਹ ਚੋਣ ਲੋੜੀਦੀ ਹੈ।\n"

#: passwd.c:339
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: ਤੁਹਾਨੂੰ ਪਛਾਣ ਨਹੀਂ ਦੇ ਸਕਦਾ!\n"

#: passwd.c:348 passwd.c:383
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: ਅਣਪਛਾਤਾ ਵਰਤੋਂਕਾਰ ਨਾਂ '%s' ਹੈ।\n"

#: passwd.c:392
#, c-format
msgid "%s: SELinux denying access due to security policy.\n"
msgstr "%s: SELinux ਨੇ ਸੁਰੱਖਿਆ ਪਾਲਸੀ ਕਰਕੇ ਪਹੁੰਚ ਉੱਤੇ ਰੋਕ ਲਾਈ।\n"

#: passwd.c:402
#, c-format
msgid "Locking password for user %s.\n"
msgstr "ਵਰਤੋਂਕਾਰ %s ਲਈ ਪਾਸਵਰਡ ਲਾਕ ਕੀਤਾ ਜਾ ਰਿਹਾ ਹੈ।\n"

#: passwd.c:406 passwd.c:418 passwd.c:433 passwd.c:444 passwd.c:464
msgid "Success"
msgstr "ਕਾਮਯਾਬ ਹੈ"

#: passwd.c:406 passwd.c:421
msgid "Error (password not set?)"
msgstr "ਗਲਤੀ (ਪਾਸਵਰਡ ਨਹੀਂ ਦਿੱਤਾ?)"

#: passwd.c:414
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "%s ਵਰਤੋਂਕਾਰ ਲਈ ਲਾਕ ਖੋਲ੍ਹਿਆ ਜਾਂਦਾ ਹੈ।\n"

#: passwd.c:420
msgid "Unsafe operation (use -f to force)"
msgstr "ਅਸੁਰੱਖਿਅਤ ਕਾਰਵਾਈ (ਧੱਕੇ ਲਈ -f ਵਰਤੋਂ)"

#: passwd.c:429
#, c-format
msgid "Expiring password for user %s.\n"
msgstr "ਵਰਤੋਂਕਾਰ %s ਲਈ ਪਾਸਵਰਡ ਦੀ ਮਿਆਦ ਪੁੱਗ ਰਹੀ ਹੈ।"

#: passwd.c:433 passwd.c:444 passwd.c:464
msgid "Error"
msgstr "ਗਲਤੀ"

#: passwd.c:441
#, c-format
msgid "Removing password for user %s.\n"
msgstr "ਵਰਤੋਂਕਾਰ %s ਲਈ ਪਾਸਵਰਡ ਹਟਾਇਆ ਜਾ ਰਿਹਾ ਹੈ।\n"

#: passwd.c:461
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "%s ਵਰਤੋਂਕਾਰ ਉਮਰ ਡਾਟਾ ਠੀਕ ਕੀਤਾ ਜਾ ਰਿਹਾ ਹੈ।\n"

#: passwd.c:477
#, c-format
msgid "Changing password for user %s.\n"
msgstr "%s ਲਈ ਪਾਸਵਰਡ ਬਦਲਿਆ ਜਾ ਰਿਹਾ ਹੈ।\n"

#: passwd.c:489
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: stdin ਤੋਂ ਪੜ੍ਹਨ ਲਈ ਅਸਫ਼ਲ: %s\n"

#: passwd.c:506
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: ਪੈਮ ਸੁਰੂ ਕਰਨ ਲਈ ਫੇਲ੍ਹ: %s\n"

#: passwd.c:519
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: pam ਲਈ tty ਦੇਣ ਲਈ ਫੇਲ੍ਹ: %s\n"

#: passwd.c:530
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: ਫੇਲ੍ਹ ਅੰਤਰਾਲ ਦੇਣ ਲਈ ਅਸਫ਼ਲ: %s\n"

#: passwd.c:548
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s ਮਿਆਦ ਪੁੱਗੇ ਪਰਮਾਣਕਿਤਾ ਟੋਕਨ ਅੱਪਡੇਟ ਕੀਤੇ ਗਏ।\n"

#: passwd.c:554
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: ਸਭ ਪਰਮਾਣਕਿਤਾ ਟੋਕਨ ਅੱਪਡੇਟ ਕੀਤੇ ਗਏ ਹਨ\n"
