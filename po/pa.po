#: libuser.c:88
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: libuser ਸ਼ੁਰੂਆਤੀ ਗਲਤੀ:"

#: libuser.c:165
msgid "Warning: unlocked password would be empty."
msgstr "ਚੇਤਾਵਨੀ: ਖੋਲ੍ਹਣ ਗੁਪਤ-ਕੋਡ ਖਾਲੀ ਹੋਵੇਗਾ।"

#: libuser.c:279
msgid "Corrupted passwd entry."
msgstr "ਨਿਕਾਰਾ ਗੁਪਤ-ਕੋਡ ਇੰਦਰਾਜ਼ ਹੈ।"

#: libuser.c:296
msgid "Empty password."
msgstr "ਖਾਲੀ ਗੁਪਤ-ਕੋਡ"

#: libuser.c:299
msgid "Password locked."
msgstr "ਗੁਪਤ-ਕੋਡ ਤਾਲਾਬੰਦ"

#: libuser.c:303
msgid "Password set, MD5 crypt."
msgstr "ਗੁਪਤ-ਕੋਡ ਦਿੱਤਾ, MD5 ਇੰਕ੍ਰਿਪਟ"

#: libuser.c:306
msgid "Password set, blowfish crypt."
msgstr "ਗੁਪਤ-ਕੋਡ ਦਿੱਤਾ, blowfish ਇੰਕ੍ਰਿਪਟ"

#: libuser.c:309
#, fuzzy
msgid "Password set, SHA256 crypt."
msgstr "ਗੁਪਤ-ਕੋਡ ਦਿੱਤਾ, MD5 ਇੰਕ੍ਰਿਪਟ"

#: libuser.c:312
#, fuzzy
msgid "Password set, SHA512 crypt."
msgstr "ਗੁਪਤ-ਕੋਡ ਦਿੱਤਾ, MD5 ਇੰਕ੍ਰਿਪਟ"

#: libuser.c:314
msgid "Password set, unknown crypt variant."
msgstr "ਗੁਪਤ-ਕੋਡ ਦਿੱਤਾ, ਅਣਜਾਣ ਇੰਕ੍ਰਿਪਟ ਗੁਣ ਹੈ।"

#: libuser.c:317
msgid "Alternate authentication scheme in use."
msgstr "ਬਦਲਵਾਂ ਪਰਮਾਣਕਿਤਾ ਸਕੀਮ ਵਰਤੀ ਗਈ ਹੈ।"

#: libuser.c:322
msgid "Password set, DES crypt."
msgstr "ਗੁਪਤ ਕੋਡ ਦਿੱਤਾ DES ਦਿੱਤਾ"

#: libuser.c:336
#, c-format
msgid "No password set.\n"
msgstr "ਕੋਈ ਗੁਪਤ-ਕੋਡ ਨੀਂ ਦਿੱਤਾ।\n"

#: libuser.c:340
#, c-format
msgid "Unknown user.\n"
msgstr "ਅਣਜਾਣ ਉਪਭੋਗੀ\n"

#: libuser.c:435
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s ਉਪਭੋਗੀ ਖਾਤਾ ਗੁਪਤ-ਕੋਡ ਉਮਰ ਨਾਲ ਸਹਿਯੋਗ ਨੀਹਂ ਹੈ।\n"

#: passwd.c:156
msgid "keep non-expired authentication tokens"
msgstr "ਨਾ-ਮਿਆਦ ਪੱਗੇ ਪਰਮਾਣਿਕਤਾ ਟੋਕਣ ਰੱਖੋ"

#: passwd.c:158
msgid "delete the password for the named account (root only)"
msgstr "ਨਾਮੀ ਖਾਤੇ (root ਲਈ) ਲਈ ਗੁਪਤ-ਕੋਡ ਹਤਾਓ"

#: passwd.c:160
msgid "lock the named account (root only)"
msgstr "ਨਾਮੀ ਖਾਤੇ (root ਲਈ) ਲਈ ਤਾਲਾ"

#: passwd.c:162
msgid "unlock the named account (root only)"
msgstr "ਨਾਮੀ ਖਾਤੇ ((root ਲਈ) ਲਈ ਤਾਲਾ-ਖੋਲ੍ਹੋ"

#: passwd.c:164
msgid "force operation"
msgstr "ਧੱਕੇ ਨਾਲ"

#: passwd.c:166
msgid "maximum password lifetime (root only)"
msgstr "ਵੱਧ ਤੋਂ ਵੱਧ ਗੁਪਤ-ਕੋਡ ਉਮਰ (root ਹੀ)"

#: passwd.c:168
msgid "minimum password lifetime (root only)"
msgstr "ਘੱਟੋ-ਘੱਟ ਗੁਪਤ-ਕੋਡ ਉਮਰ  (root ਹੀ)"

#: passwd.c:170
msgid ""
"number of days warning users receives before password expiration (root only)"
msgstr "ਗੁਪਤ-ਕੋਡ ਦੀ ਮਿਆਦ ਪੁੱਗਣ (root ਲਈ) ਤੋਂ ਪਹਿਲਾਂ ਉਪਭੋਗੀਆਂ ਨੂੰ ਚੇਤਾਵਨੀ ਦੇਣ ਲਈ ਦਿਨ"

#: passwd.c:173
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr "ਖਾਤਾ ਬੰਦ ਕਰਨ (root ਲਈ) ਲਈ ਮਿਆਦ ਪੁੱਗਣ ਬਾਰੇ ਦਿਨ ਬਾਰੇ ਦੱਸ ਜੀ"

#: passwd.c:176
msgid "report password status on the named account (root only)"
msgstr "ਨਾਮੀ ਖਾਤੇ (root ਲਈ) ਲਈ ਗੁਪਤ-ਕੋਡ ਹਾਲਤ ਰਿਪੋਰਟ ਕਰੋ"

#: passwd.c:178
msgid "read new tokens from stdin (root only)"
msgstr "stdin (root only) ਤੋਂ ਨਵੇਂ ਟੋਕਨ ਪੜ੍ਹੋ"

#: passwd.c:185
msgid "[OPTION...] <accountName>"
msgstr "[OPTION...] <accountName>"

#: passwd.c:188
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: ਗਲਤ ਮੁੱਲ%s: %s\n"

#: passwd.c:246
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s: Only one of -l, -u, -d, -S may be specified.\n"

#: passwd.c:255
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"

#: passwd.c:279
#, c-format
msgid "Only root can do that.\n"
msgstr "ਸਿਰਫ root ਹੀ ਕਰ ਸਕਦਾ ਹੈ।\n"

#: passwd.c:292
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: root ਹੀ ਇੱਕ ਉਪਭੋਗੀ ਨਾਂ ਦੇ ਸਕਦਾ ਹੈ।\n"

#: passwd.c:301
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: ਉਪਭੋਗੀ ਨਾਂ  ਬਹੁਤ ਲੰਮਾ ਹੈ\n"

#: passwd.c:311
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: ਸਿਰਫ਼ ਇੱਕ ਉਪਭੋਗੀ ਦਿੱਤਾ ਜਾ ਸਕਦਾ ਹੈ\n"

#: passwd.c:321
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: ਉਪਭੋਗੀ ਨਾਂ ਨਾਲ ਇਹ ਚੋਣ ਲੋੜੀਦੀ ਹੈ।\n"

#: passwd.c:332
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: ਤੁਹਾਨੂੰ ਪਛਾਣ ਨਹੀਂ ਦੇ ਸਕਦਾ!\n"

#: passwd.c:341 passwd.c:376
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: ਅਣਜਾਣ ਉਪਭੋਗੀ ਨਾਂ '%s'\n"

#: passwd.c:393
#, c-format
msgid "%s: %s is not authorized to change the password of %s\n"
msgstr "%s: %s %s ਲਈ ਗੁਪਤ-ਕੋਡ ਬਦਲਣ ਲਈ ਸਹੀਂ ਨਹੀਂ ਹੈ\n"

#: passwd.c:406
#, c-format
msgid "Locking password for user %s.\n"
msgstr "ਉਪਭੋਗੀ %s ਲਈ ਗੁਪਤ-ਕੋਡ ਤਾਲਾਬੰਦ ਹੈ\n"

#: passwd.c:410 passwd.c:422 passwd.c:436 passwd.c:456
msgid "Success"
msgstr "ਠੀਕ"

#: passwd.c:410 passwd.c:425
msgid "Error (password not set?)"
msgstr "ਗਲਤੀ (ਗੁਪਤ-ਕੋਡ ਨਹੀਂ ਦਿੱਤਾ?)"

#: passwd.c:418
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "%s ਉਪਭੋਗੀ ਲਈ ਤਾਲਾ ਖੋਲ੍ਹਿਆ ਜਾਂਦਾ ਹੈ\n"

#: passwd.c:424
msgid "Unsafe operation (use -f to force)"
msgstr "ਅਸੁਰੱਖਿਅਤ ਕਾਰਵਾਈ (ਧੱਕੇ ਲਈ -f ਵਰਤੋਂ)"

#: passwd.c:433
#, c-format
msgid "Removing password for user %s.\n"
msgstr "ਉਪਭੋਗੀ %s ਲਈ ਗੁਪਤਕੋਡ ਹਟਓ\n"

#: passwd.c:436 passwd.c:456
msgid "Error"
msgstr "ਗਲਤੀ"

#: passwd.c:453
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "%s ਉਪਭੋਗੀ ਲਈ ਉਮਰ ਡਾਟਾ ਠੀਕ ਕੀਤਾ ਜਾ ਰਿਹਾ ਹੈ\n"

#: passwd.c:469
#, c-format
msgid "Changing password for user %s.\n"
msgstr "%s ਲਈ ਗੁਪਤ-ਕੋਡ ਬਦਲਿਆ ਜਾ ਰਿਹਾ ਹੈ\n"

#: passwd.c:481
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: stdin ਤੋਂ ਪੜ੍ਹਨ ਲਈ ਅਸਫ਼ਲ: %s\n"

#: passwd.c:498
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: ਪੈਮ ਸੁਰੂ ਕਰਨ ਲਈ ਫੇਲ੍ਹ: %s\n"

#: passwd.c:511
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: ਪੈਮ ਲਈ ਟੀਟੀਯਾਈ ਦੇਣ ਲਈ ਫੇਲ੍ਹ: %s\n"

#: passwd.c:522
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: ਫੇਲ੍ ਅੰਤਰਾਲ ਦੇਣ ਲਈ ਅਸਫ਼ਲ: %s\n"

#: passwd.c:540
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s ਮਿਆਦ ਪੁੱਗੇ ਪਰਮਾਣਕਿਤਾ ਟੋਕਨ ਅੱਪਡੇਟ ਕੀਤੇ ਗਏ।\n"

#: passwd.c:546
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: ਸਭ ਪਰਮਾਣਕਿਤਾ ਟੋਕਨ ਅੱਪਡੇਟ ਕੀਤੇ ਗਏ ਹਨ\n"
