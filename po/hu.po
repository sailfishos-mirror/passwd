#: libuser.c:88
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: Libuser-inicializációs hiba:"

#: libuser.c:165
msgid "Warning: unlocked password would be empty."
msgstr "Figyelmeztetés: a kioldott jelszó üres lenne."

#: libuser.c:279
msgid "Corrupted passwd entry."
msgstr "Sérült jelszóbejegyzés."

#: libuser.c:296
msgid "Empty password."
msgstr "Üres jelszó."

#: libuser.c:299
msgid "Password locked."
msgstr "Jelszó zárolva."

#: libuser.c:303
msgid "Password set, MD5 crypt."
msgstr "Jelszó beállítva, MD5-titkosítás."

#: libuser.c:306
msgid "Password set, blowfish crypt."
msgstr "Jelszó beállítva, Blowfish-titkosítás."

#: libuser.c:309
msgid "Password set, SHA256 crypt."
msgstr "Jelszó beállítva, SHA256 titkosítás."

#: libuser.c:312
msgid "Password set, SHA512 crypt."
msgstr "Jelszó beállítva, SHA512 titkosítás."

#: libuser.c:314
msgid "Password set, unknown crypt variant."
msgstr "Jelszó beállítva, ismeretlen titkosítási változat."

#: libuser.c:317
msgid "Alternate authentication scheme in use."
msgstr "Váltakozó azonosítási séma van használva."

#: libuser.c:322
msgid "Password set, DES crypt."
msgstr "Jelszó beállítva, DES-titkosítás."

#: libuser.c:336
#, c-format
msgid "No password set.\n"
msgstr "Nincs jelszó beállítva.\n"

#: libuser.c:340
#, c-format
msgid "Unknown user.\n"
msgstr "Ismeretlen felhasználó.\n"

#: libuser.c:435
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr ""
"%s: a felhasználó számlája nem támogatja a jelszólejárat-beállítást "
"(aging).\n"

#: passwd.c:156
msgid "keep non-expired authentication tokens"
msgstr "nem lejárt azonosítási jegyek (token) megtartása"

#: passwd.c:158
msgid "delete the password for the named account (root only)"
msgstr "a nevesített számla jelszavának törlése (csak rendszergazda)"

#: passwd.c:160
msgid "lock the named account (root only)"
msgstr "a nevesített számla zárolása (csak rendszergazda)"

#: passwd.c:162
msgid "unlock the named account (root only)"
msgstr "a nevesített számla kioldása (csak rendszergazda)"

#: passwd.c:164
msgid "force operation"
msgstr "művelet kikényszerítése"

#: passwd.c:166
msgid "maximum password lifetime (root only)"
msgstr "jelszó maximális élettartama (csak rendszergazda)"

#: passwd.c:168
msgid "minimum password lifetime (root only)"
msgstr "jelszó minimális élettartama (csak rendszergazda)"

#: passwd.c:170
msgid ""
"number of days warning users receives before password expiration (root only)"
msgstr ""
"jelszó lejárta előtti napok száma, amikortól figyelmeztetést kap a használó "
"(csak rendszergazda)"

#: passwd.c:173
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr ""
"jelszó lejárta utáni napok száma, amikortól a számla letiltásra kerül (csak "
"rendszergazda)"

#: passwd.c:176
msgid "report password status on the named account (root only)"
msgstr "jelentés a nevesített számla jelszóállapotáról (csak rendszergazda)"

#: passwd.c:178
msgid "read new tokens from stdin (root only)"
msgstr "új tokenek beolvasása a standard bemenetről (csak rendszergazda)"

#: passwd.c:185
msgid "[OPTION...] <accountName>"
msgstr "[OPCIÓ...] <számlaNév>"

#: passwd.c:188
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: rossz argumentum: %s: %s\n"

#: passwd.c:246
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s: -l, -u, -d, -S kapcsolókból csak az egyiket szabad megadni.\n"

#: passwd.c:255
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr ""
"%s: -l, -u, -d, -S egyikét sem lehet keverni ezekkel: -i, -n, -w, -x.\n"

#: passwd.c:279
#, c-format
msgid "Only root can do that.\n"
msgstr "Ezt csak a rendszergazda teheti meg.\n"

#: passwd.c:292
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: Csak a rendszergazda adhat meg felhasználónevet.\n"

#: passwd.c:301
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: Az adott felhasználónév túl hosszú.\n"

#: passwd.c:311
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: Csak egy felhasználónevet szabad megadni.\n"

#: passwd.c:321
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: Ezen opció felhasználónevet igényel.\n"

#: passwd.c:332
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: Ön nem azonosítható!\n"

#: passwd.c:341 passwd.c:376
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: \"%s\": ismeretlen felhasználónév.\n"

#: passwd.c:393
#, c-format
msgid "%s: %s is not authorized to change the password of %s\n"
msgstr "%s: %s számára nem engedélyezett %s jelszavának megváltoztatása\n"

#: passwd.c:406
#, c-format
msgid "Locking password for user %s.\n"
msgstr "%s felhasználó jelszavának zárolása.\n"

#: passwd.c:410 passwd.c:422 passwd.c:436 passwd.c:456
msgid "Success"
msgstr "Sikerült"

#: passwd.c:410 passwd.c:425
msgid "Error (password not set?)"
msgstr "Hiba (jelszó nincs beállítva?)"

#: passwd.c:418
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "%s felhasználó jelszavának kioldása.\n"

#: passwd.c:424
msgid "Unsafe operation (use -f to force)"
msgstr "Nem biztonságos művelet (kikényszerítés: -f)"

#: passwd.c:433
#, c-format
msgid "Removing password for user %s.\n"
msgstr "%s felhasználó jelszavának eltávolítása.\n"

#: passwd.c:436 passwd.c:456
msgid "Error"
msgstr "Hiba"

#: passwd.c:453
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "%s felhasználó jelszólejárati (aging) adatainak beállítása.\n"

#: passwd.c:469
#, c-format
msgid "Changing password for user %s.\n"
msgstr "%s felhasználó jelszavának megváltoztatása.\n"

#: passwd.c:481
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: hiba a standard bemenet olvasásakor: %s\n"

#: passwd.c:498
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: a PAM nem indítható: %s\n"

#: passwd.c:511
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: nem lehet tty-t beállítani a PAM-hoz: %s\n"

#: passwd.c:522
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: nem lehet hiba-késleltetést beállítani: %s\n"

#: passwd.c:540
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s: a lejárt azonosítási jegyek frissítése sikerült.\n"

#: passwd.c:546
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: minden azonosítási jegy frissítése sikerült.\n"
