#: libuser.c:90
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: libuser inicializációs hiba:"

#: libuser.c:167
msgid "Warning: unlocked password would be empty."
msgstr "Figyelmezetetés: a kioldott jelszó üres lenne."

#: libuser.c:281
msgid "Corrupted passwd entry."
msgstr "Sérült jelszóbejegyzés."

#: libuser.c:298
msgid "Empty password."
msgstr "Üres jelszó."

#: libuser.c:301
msgid "Password locked."
msgstr "Jelszó zárolva."

#: libuser.c:305
msgid "Password set, MD5 crypt."
msgstr "Jelszó beállítva, MD5 titk."

#: libuser.c:308
msgid "Password set, blowfish crypt."
msgstr "jelszó beállítva, blowfish titk."

#: libuser.c:310
msgid "Password set, unknown crypt variant."
msgstr "Jelszó beállítva, ismeretlen titk. változat."

#: libuser.c:313
msgid "Alternate authentication scheme in use."
msgstr "Váltakozó azonosítási séma haszált."

#: libuser.c:318
msgid "Password set, DES crypt."
msgstr "Jelszó beállítva, DES titk."

#: libuser.c:332
#, c-format
msgid "No password set.\n"
msgstr "Nincs jelszó beállítva.\n"

#: libuser.c:336
#, c-format
msgid "Unknown user.\n"
msgstr "Ismeretlen használó.\n"

#: libuser.c:431
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s: használó számlája nem támogatja a jelszó öregítést.\n"

#: passwd.c:158
msgid "keep non-expired authentication tokens"
msgstr "nem lejárt azonosítási jegyek megtartása"

#: passwd.c:160
msgid "delete the password for the named account (root only)"
msgstr "a nevezett számla jelszavának törlése (csak root)"

#: passwd.c:162
msgid "lock the named account (root only)"
msgstr "nevezett számla zárolása (csak root)"

#: passwd.c:164
msgid "unlock the named account (root only)"
msgstr "nevezett számla kioldása (csak root)"

#: passwd.c:166
msgid "force operation"
msgstr "művelet kikényszerítése"

#: passwd.c:168
msgid "maximum password lifetime (root only)"
msgstr "jelszó leghosszabb élettartama (csak root)"

#: passwd.c:170
msgid "minimum password lifetime (root only)"
msgstr "jelszó legrövidebb élettartama (csak root)"

#: passwd.c:172
msgid ""
"number of days warning users receives before password expiration (root only)"
msgstr ""
"jelszó lejárta előtti napok száma, amikortól figyelmeztetést kap a használó (csak root)"

#: passwd.c:175
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr ""
"jelszó lejárta utáni napok száma, amikortól a számla letiltásra kerül (csak root)"

#: passwd.c:178
msgid "report password status on the named account (root only)"
msgstr "a nevezett számla jelszó állapotának jelentése (csak root)"

#: passwd.c:180
msgid "read new tokens from stdin (root only)"
msgstr "új tokenek beolvasása stdin-ről (csak root)"

#: passwd.c:187
msgid "[OPTION...] <accountName>"
msgstr "[EXTRA…] <számlaNév>"

#: passwd.c:190
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: rossz argumentum %s: %s\n"

#: passwd.c:248
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s: -l, -u, -d, -S kapcsolókból csak az egyiket szabad megadni.\n"

#: passwd.c:257
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr "%s: -l, -u, -d, -S egyikét sem lehet keverni ezekkel: -i, -n, -w, -x.\n"

#: passwd.c:281
#, c-format
msgid "Only root can do that.\n"
msgstr "Ezt csak root teheti meg.\n"

#: passwd.c:294
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: Csak root adhatja meg használó nevét.\n"

#: passwd.c:303
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: Az adott használói név túl hosszú.\n"

#: passwd.c:313
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: Csak egy használói nevet szabad megadni.\n"

#: passwd.c:323
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: Ezen extra használói nevet igényel.\n"

#: passwd.c:334
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: Ön nem azonosítható!\n"

#: passwd.c:343 passwd.c:378
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: „%s” ismeretlen használói név.\n"

#: passwd.c:395
#, c-format
msgid "%s: %s is not authorized to change the password of %s\n"
msgstr "%s: %s számára nem engedélyezett %s jelszavának megváltoztatása\n"

#: passwd.c:408
#, c-format
msgid "Locking password for user %s.\n"
msgstr "%s használó jelszavának zárolása.\n"

#: passwd.c:412 passwd.c:424 passwd.c:438 passwd.c:458
msgid "Success"
msgstr "Sikerült"

#: passwd.c:412 passwd.c:427
msgid "Error (password not set?)"
msgstr "Hiba (jelszó nincs beállítva?)"

#: passwd.c:420
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "%s használó jelszavának kioldása.\n"

#: passwd.c:426
msgid "Unsafe operation (use -f to force)"
msgstr "Nem biztonságos művelet (erőltesse -f-fel)"

#: passwd.c:435
#, c-format
msgid "Removing password for user %s.\n"
msgstr "%s használó jelszavának eltávolítása.\n"

#: passwd.c:438 passwd.c:458
msgid "Error"
msgstr "Hiba"

#: passwd.c:455
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "%s használó öregítési adatainak igazítása.\n"

#: passwd.c:471
#, c-format
msgid "Changing password for user %s.\n"
msgstr "%s használó jelszavának megváltoztatása.\n"

#: passwd.c:483
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: hiba stdint olvasva: %s\n"

#: passwd.c:500
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: pam nem indítható: %s\n"

#: passwd.c:513
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: nem lehet tty-t beállítani pamhoz: %s\n"

#: passwd.c:524
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: nem lehet hiba késleltetését beállítani: %s\n"

#: passwd.c:542
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s: a lejárt azonosítási jegyek időszerűsítése sikerült.\n"

#: passwd.c:548
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: minden azonosítási jegy időszerűsítése sikerült.\n"
