#: libuser.c:88
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: errore inizializzazione libuser:"

#: libuser.c:165
msgid "Warning: unlocked password would be empty."
msgstr "Attenzione: la password sbloccata sarà vuota."

#: libuser.c:279
msgid "Corrupted passwd entry."
msgstr "Voce passwd corrotta."

#: libuser.c:296
msgid "Empty password."
msgstr "Password vuota."

#: libuser.c:299
msgid "Password locked."
msgstr "Password bloccata."

#: libuser.c:303
msgid "Password set, MD5 crypt."
msgstr "Password impostata, crittazione MD5."

#: libuser.c:306
msgid "Password set, blowfish crypt."
msgstr "Password impostata, crittazione blowfish."

#: libuser.c:309
msgid "Password set, SHA256 crypt."
msgstr "Password impostata, crittazione SHA256."

#: libuser.c:312
msgid "Password set, SHA512 crypt."
msgstr "Password impostata, crittazione SHA512."

#: libuser.c:314
msgid "Password set, unknown crypt variant."
msgstr "Password impostata, variante di crittazione sconosciuta."

#: libuser.c:317
msgid "Alternate authentication scheme in use."
msgstr "Schema di autenticazione alternativo in uso."

#: libuser.c:322
msgid "Password set, DES crypt."
msgstr "Password impostata, crittazione DES."

#: libuser.c:336
#, c-format
msgid "No password set.\n"
msgstr "Nessuna password impostata.\n"

#: libuser.c:340
#, c-format
msgid "Unknown user.\n"
msgstr "Utente sconosciuto.\n"

#: libuser.c:435
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s: l'account utente non supporta la scadenza password.\n"

#: passwd.c:156
msgid "keep non-expired authentication tokens"
msgstr "mantieni gli authentication token non scaduti"

#: passwd.c:158
msgid "delete the password for the named account (root only)"
msgstr "elimina la password per l'account named (solo root)"

#: passwd.c:160
msgid "lock the named account (root only)"
msgstr "blocca l'account named (solo root)"

#: passwd.c:162
msgid "unlock the named account (root only)"
msgstr "sblocca l'account named (solo root)"

#: passwd.c:164
msgid "force operation"
msgstr "forza operazione"

#: passwd.c:166
msgid "maximum password lifetime (root only)"
msgstr "durata massima della password (solo root)"

#: passwd.c:168
msgid "minimum password lifetime (root only)"
msgstr "durata minima della password (solo root)"

#: passwd.c:170
msgid ""
"number of days warning users receives before password expiration (root only)"
msgstr ""
"numero di giorni di ricezione avvertimento dell'utente prima della scadenza "
"della password (solo root)"

#: passwd.c:173
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr ""
"numero di giorni dopo la scadenza della password in cui un account viene "
"disabilitato (solo root)"

#: passwd.c:176
msgid "report password status on the named account (root only)"
msgstr "mostra lo status della password per l'account named (solo root)"

#: passwd.c:178
msgid "read new tokens from stdin (root only)"
msgstr "leggi i nuovi token dallo stdin (solo root)"

#: passwd.c:185
msgid "[OPTION...] <accountName>"
msgstr "[OPZIONE...] <accountName>"

#: passwd.c:188
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: argomento errato %s: %s\n"

#: passwd.c:246
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s: Solo uno tra -l, -u, -d, -S può essere specificato.\n"

#: passwd.c:255
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr "%s: Non si può mescolare un -l, -u, -d, -S con un -i, -n, -w, -x.\n"

#: passwd.c:279
#, c-format
msgid "Only root can do that.\n"
msgstr "Solo root può farlo.\n"

#: passwd.c:292
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: Solo root può specificare un nome utente.\n"

#: passwd.c:301
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: Il nome utente fornito è troppo lungo.\n"

#: passwd.c:311
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: Solo un nome utente può essere specificato.\n"

#: passwd.c:321
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: Questa opzione richiede un nome utente.\n"

#: passwd.c:332
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: Non può identificarti!\n"

#: passwd.c:341 passwd.c:376
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: Nome utente sconosciuto '%s'.\n"

#: passwd.c:393
#, c-format
msgid "%s: %s is not authorized to change the password of %s\n"
msgstr "%s: %s non è autorizzato a cambiare la password di %s\n"

#: passwd.c:406
#, c-format
msgid "Locking password for user %s.\n"
msgstr "Blocco della password per l'utente %s.\n"

#: passwd.c:410 passwd.c:422 passwd.c:436 passwd.c:456
msgid "Success"
msgstr "Successo"

#: passwd.c:410 passwd.c:425
msgid "Error (password not set?)"
msgstr "Errore (password non impostata?)"

#: passwd.c:418
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "Sblocco della password per l'utente %s.\n"

#: passwd.c:424
msgid "Unsafe operation (use -f to force)"
msgstr "Operazione non sicura (usare -f per forzare)"

#: passwd.c:433
#, c-format
msgid "Removing password for user %s.\n"
msgstr "Rimozione password per l'utente %s.\n"

#: passwd.c:436 passwd.c:456
msgid "Error"
msgstr "Errore"

#: passwd.c:453
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "Impostazione data di scadenza per l'utente %s.\n"

#: passwd.c:469
#, c-format
msgid "Changing password for user %s.\n"
msgstr "Cambio password per l'utente %s.\n"

#: passwd.c:481
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: errore nella lettura dallo stdin: %s\n"

#: passwd.c:498
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: impossibile avviare pam: %s\n"

#: passwd.c:511
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: impossibile impostare una tty per pam: %s\n"

#: passwd.c:522
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: impossibile impostare un ritardo per il fallimento: %s\n"

#: passwd.c:540
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s: authentication token scaduti aggiornati con successo.\n"

#: passwd.c:546
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr ""
"%s: tutti gli authentication token sono stati aggiornati con successo.\n"
