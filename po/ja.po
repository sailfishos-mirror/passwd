#: libuser.c:90
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: libuser 初期化エラー:"

#: libuser.c:167
msgid "Warning: unlocked password would be empty."
msgstr "警告:ロック解除したパスワードは空です。"

#: libuser.c:281
msgid "Corrupted passwd entry."
msgstr "passwd 入力の文字化け"

#: libuser.c:298
msgid "Empty password."
msgstr "空のパスワード"

#: libuser.c:301
msgid "Password locked."
msgstr "パスワードはロック済み。"

#: libuser.c:305
msgid "Password set, MD5 crypt."
msgstr "パスワード設定済み、MD5 暗号化。"

#: libuser.c:308
msgid "Password set, blowfish crypt."
msgstr "パスワード設定済み、blowfish 暗号化。"

#: libuser.c:310
msgid "Password set, unknown crypt variant."
msgstr "パスワード設定済み、不明な暗号化変種。"

#: libuser.c:313
msgid "Alternate authentication scheme in use."
msgstr "代替の認証方針が使用されています。"

#: libuser.c:318
msgid "Password set, DES crypt."
msgstr "パスワード設定済み、DES 暗号化"

#: libuser.c:332
#, c-format
msgid "No password set.\n"
msgstr "パスワードの設定なし。\n"

#: libuser.c:336
#, c-format
msgid "Unknown user.\n"
msgstr "不明なユーザー。\n"

#: libuser.c:431
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s: ユーザーアカウントにはパスワードエージングのサポートがありません。\n"

#: passwd.c:158
msgid "keep non-expired authentication tokens"
msgstr "有効な認証トークンを維持する"

#: passwd.c:160
msgid "delete the password for the named account (root only)"
msgstr "named アカウントのパスワードを削除する (root のみ)"

#: passwd.c:162
msgid "lock the named account (root only)"
msgstr "named アカウントをロックする (root のみ)"

#: passwd.c:164
msgid "unlock the named account (root only)"
msgstr "named アカウントをロック解除する (root のみ)"

#: passwd.c:166
msgid "force operation"
msgstr "操作を強制"

#: passwd.c:168
msgid "maximum password lifetime (root only)"
msgstr "最大パスワード寿命 (root のみ)"

#: passwd.c:170
msgid "minimum password lifetime (root only)"
msgstr "最低パスワード寿命 (root のみ)"

#: passwd.c:172
msgid "number of days warning users receives before password expiration (root only)"
msgstr "パスワード失効の前にユーザーが警告を受ける事前日数 (root のみ)"

#: passwd.c:175
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr "パスワードが失効した後にアカウントが無効になるまでの日数 (root のみ)"

#: passwd.c:178
msgid "report password status on the named account (root only)"
msgstr "named アカウントのパスワードステータスを報告する (root のみ)"

#: passwd.c:180
msgid "read new tokens from stdin (root only)"
msgstr "標準入力(stdin) から新規トークンを読み込む (root のみ)"

#: passwd.c:187
msgid "[OPTION...] <accountName>"
msgstr "[オプション...] <accountName>"

#: passwd.c:190
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: 不正な引数 %s: %s\n"

#: passwd.c:248
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s: -l, -u, -d, -S の内１つのみが指定可能です。\n"

#: passwd.c:257
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr "%s: -l, -u, -d, -S の１つと -i, -n, -w, -x の１つを混合できません。\n"

#: passwd.c:281
#, c-format
msgid "Only root can do that.\n"
msgstr "root のみが実行できます。\n"

#: passwd.c:294
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: root のみがユーザー名を指定できます。\n"

#: passwd.c:303
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: 入力されたユーザー名は長すぎます。\n"

#: passwd.c:313
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: ユーザー名のみが指定可能です。\n"

#: passwd.c:323
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: このオプションにはユーザー名が必要です。\n"

#: passwd.c:334
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: あなたを識別できません! \n"

#: passwd.c:343 passwd.c:378
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: 不明なユーザー名 '%s'。\n"

#: passwd.c:395
#, c-format
msgid "%s: %s is not authorized to change the password of %s\n"
msgstr "%s: %s は %s のパスワードを変更する権限を持ちません。\n"

#: passwd.c:408
#, c-format
msgid "Locking password for user %s.\n"
msgstr "ユーザー %s 用のパスワードをロック。\n"

#: passwd.c:412 passwd.c:424 passwd.c:438 passwd.c:458
msgid "Success"
msgstr "成功"

#: passwd.c:412 passwd.c:427
msgid "Error (password not set?)"
msgstr "エラー (パスワード設定なし?)"

#: passwd.c:420
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "ユーザー %s 用のパスワードをロック解除。\n"

#: passwd.c:426
msgid "Unsafe operation (use -f to force)"
msgstr "危険な操作 (強制には -f を使用)"

#: passwd.c:435
#, c-format
msgid "Removing password for user %s.\n"
msgstr "ユーザー %s のパスワードを削除。 \n"

#: passwd.c:438 passwd.c:458
msgid "Error"
msgstr "エラー"

#: passwd.c:455
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "ユーザー %s のエージングデータを調節。\n"

#: passwd.c:471
#, c-format
msgid "Changing password for user %s.\n"
msgstr "ユーザー %s のパスワードを変更。\n"

#: passwd.c:483
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: 標準入力(stdin) からの読み込みエラー: %s\n"

#: passwd.c:500
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: pam の起動ができません: %s\n"

#: passwd.c:513
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: pam 用の tty がセットできません: %s\n"

#: passwd.c:524
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: 失敗遅延を設定できません: %s\n"

#: passwd.c:542
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s: 失効した認証トークンが正しく更新できました。\n"

#: passwd.c:548
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: 全ての認証トークンが正しく更新できました。\n"

