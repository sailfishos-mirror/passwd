#: libuser.c:88
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: napaka pri inicializaciji libuser:"

#: libuser.c:165
msgid "Warning: unlocked password would be empty."
msgstr "Opozorilo: odklenjeno geslo bo prazno."

#: libuser.c:279
msgid "Corrupted passwd entry."
msgstr "Okvarjeni zapis za geslo."

#: libuser.c:296
msgid "Empty password."
msgstr "Prazno geslo."

#: libuser.c:299
msgid "Password locked."
msgstr "Zaklenjeno geslo."

#: libuser.c:303
msgid "Password set, MD5 crypt."
msgstr "Nastavljeno geslo v oblika MD5."

#: libuser.c:306
msgid "Password set, blowfish crypt."
msgstr "Nastavljeno geslo v obliki blowfish."

#: libuser.c:309
#, fuzzy
msgid "Password set, SHA256 crypt."
msgstr "Nastavljeno geslo v oblika MD5."

#: libuser.c:312
#, fuzzy
msgid "Password set, SHA512 crypt."
msgstr "Nastavljeno geslo v oblika MD5."

#: libuser.c:314
msgid "Password set, unknown crypt variant."
msgstr "Nastavljeno geslo v neznani obliki."

#: libuser.c:317
msgid "Alternate authentication scheme in use."
msgstr "V veljavi je druga shema za preverjanje istovetnosti."

#: libuser.c:322
msgid "Password set, DES crypt."
msgstr "Nastavljeno geslo v obliki DES."

#: libuser.c:336
#, c-format
msgid "No password set.\n"
msgstr "Geslo ni nastavljeno.\n"

#: libuser.c:340
#, c-format
msgid "Unknown user.\n"
msgstr "Neznan uporabnik.\n"

#: libuser.c:435
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s: uporabniški račun ne podpira zastaranja gesla.\n"

#: passwd.c:156
msgid "keep non-expired authentication tokens"
msgstr "obdrži nezastarane žetone za preverjanje istovetnosti"

#: passwd.c:158
msgid "delete the password for the named account (root only)"
msgstr "odstrani geslo za omenjeni račun (samo root)"

#: passwd.c:160
msgid "lock the named account (root only)"
msgstr "zakleni omenjeni račun (samo root)"

#: passwd.c:162
msgid "unlock the named account (root only)"
msgstr "odkleni omenjeni račun (samo root)"

#: passwd.c:164
msgid "force operation"
msgstr "prisili operacijo"

#: passwd.c:166
msgid "maximum password lifetime (root only)"
msgstr "največja življenska doba gesla (samo root)"

#: passwd.c:168
msgid "minimum password lifetime (root only)"
msgstr "najmanjša življenska doba gesla (samo root)"

#: passwd.c:170
msgid ""
"number of days warning users receives before password expiration (root only)"
msgstr "število dni opozarjanja preden poteče geslo (samo root)"

#: passwd.c:173
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr "število dni opozarjanja preden se račun zaklene (samo root)"

#: passwd.c:176
msgid "report password status on the named account (root only)"
msgstr "poročilo o stanju gesla za omenjeni račun (samo root)"

#: passwd.c:178
msgid "read new tokens from stdin (root only)"
msgstr "preberi nove žetone iz standardnega vhoda (samo root)"

#: passwd.c:185
msgid "[OPTION...] <accountName>"
msgstr "[Zmožnost...] <imeRačuna>"

#: passwd.c:188
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: slab parameter %s: %s\n"

#: passwd.c:246
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s: Samo eno stikalo izmed -l, -u, -d, -S sme biti nastavljeno.\n"

#: passwd.c:255
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr ""
"%s: -l, -u, -d, -S  se ne smejo mešati s katerikoli izmed -i, -n, -w, -x.\n"

#: passwd.c:279
#, c-format
msgid "Only root can do that.\n"
msgstr "Samo root sme to storiti.\n"

#: passwd.c:292
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: Samo root sme podatki uporabniško ime.\n"

#: passwd.c:301
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: Navedeno uporabniško ime je predolgo.\n"

#: passwd.c:311
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: Dovoljeno je navesti le eno uporabniško ime.\n"

#: passwd.c:321
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: Zmožnost zahteva navedbo uporabniškega imena.\n"

#: passwd.c:332
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: Ne morem vas prepoznati!\n"

#: passwd.c:341 passwd.c:376
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: Neznano uporabniško ime '%s'.\n"

#: passwd.c:393
#, c-format
msgid "%s: %s is not authorized to change the password of %s\n"
msgstr "%s: %s ni avtoriziran za spreminjanje gesla %s\n"

#: passwd.c:406
#, c-format
msgid "Locking password for user %s.\n"
msgstr "Zaklepam geslo za uporabnika %s.\n"

#: passwd.c:410 passwd.c:422 passwd.c:436 passwd.c:456
msgid "Success"
msgstr "Uspeh"

#: passwd.c:410 passwd.c:425
msgid "Error (password not set?)"
msgstr "Napaka (geslo ni nastavljeno?)"

#: passwd.c:418
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "Odklepam geslo za uporabnika %s.\n"

#: passwd.c:424
msgid "Unsafe operation (use -f to force)"
msgstr "Mevarna operacija (uporabi -f za prisilo)"

#: passwd.c:433
#, c-format
msgid "Removing password for user %s.\n"
msgstr "Uporabniku %s odstranjujem geslo.\n"

#: passwd.c:436 passwd.c:456
msgid "Error"
msgstr "Napaka"

#: passwd.c:453
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "Spreminjam podatke o zastaranju za uporabnika %s.\n"

#: passwd.c:469
#, c-format
msgid "Changing password for user %s.\n"
msgstr "Spreminjam geslo uporabniku %s.\n"

#: passwd.c:481
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: napaka pri branju z standardnega vhoda: %s\n"

#: passwd.c:498
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: pam ni bilo moč zagnati: %s\n"

#: passwd.c:511
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: ni bilo moč nastaviti tty za pam: %s\n"

#: passwd.c:522
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: ni bilo moč nastaviti zakasnitev ob napaki: %s\n"

#: passwd.c:540
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr ""
"%s: potečeni žetoni za preverjanje istovetnosti so uspešno posodobljeni.\n"

#: passwd.c:546
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: VSI žetoni za preverjanje istovetnosti so uspešno posodobljeni.\n"
