#: libuser.c:88
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: error de inicialización de libuser:"

#: libuser.c:165
msgid "Warning: unlocked password would be empty."
msgstr "Advertencia: el archivo de claves desbloqueado puede estar vacío."

#: libuser.c:279
msgid "Corrupted passwd entry."
msgstr "Entrada de passwd corrupta."

#: libuser.c:296
msgid "Empty password."
msgstr "Contraseña vacía."

#: libuser.c:299
msgid "Password locked."
msgstr "Contraseña bloqueada."

#: libuser.c:303
msgid "Password set, MD5 crypt."
msgstr "Contraseña puesta, encriptado MD5."

#: libuser.c:306
msgid "Password set, blowfish crypt."
msgstr "Contraseña puesta, encriptado blowfish"

