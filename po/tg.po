#: libuser.c:91
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: хатои омодасозии libuser:"

#: libuser.c:157
msgid "Warning: unlocked password would be empty."
msgstr "Огоҳӣ: пароли кушодашуда холӣ мешавад."

#: libuser.c:203
msgid "Note: deleting a password also unlocks the password."
msgstr ""

#: libuser.c:267
msgid "Corrupted passwd entry."
msgstr "Вориди пароли вайроншуда."

#: libuser.c:284
msgid "Empty password."
msgstr "Парол холӣ аст."

#: libuser.c:287
msgid "Password locked."
msgstr "Парол баста шуд."

#: libuser.c:291
msgid "Password set, MD5 crypt."
msgstr "Парол сабт шуд, рамзгузории MD5."

#: libuser.c:294
msgid "Password set, blowfish crypt."
msgstr "Парол сабт шуд, рамзгузории blowfish."

#: libuser.c:297
msgid "Password set, SHA256 crypt."
msgstr "Парол сабт шуд, рамзгузории SHA256."

#: libuser.c:300
msgid "Password set, SHA512 crypt."
msgstr "Парол сабт шуд, рамзгузории SHA512."

#: libuser.c:302
msgid "Password set, unknown crypt variant."
msgstr "Парол сабт шуд, рамзгузории варианти номаълум."

#: libuser.c:305
msgid "Alternate authentication scheme in use."
msgstr "Нақшаи аслшиносии эҳтиётӣ дар истифода аст."

#: libuser.c:310
msgid "Password set, DES crypt."
msgstr "Парол сабт шуд, рамзгузории DES."

#: libuser.c:323
#, c-format
msgid "No password set.\n"
msgstr "Парол танзим нашудааст.\n"

#: libuser.c:328
#, c-format
msgid "Unknown user.\n"
msgstr "Корбари номаълум.\n"

#: libuser.c:403
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s: ҳисоби корбар кӯҳнашавии паролро дастгирӣ намекунад.\n"

#: passwd.c:158
msgid "keep non-expired authentication tokens"
msgstr "рамзҳои санҷиши аслияти бе муддатро нигоҳ доред"

#: passwd.c:160
msgid ""
"delete the password for the named account (root only); also removes password "
"lock if any"
msgstr ""

#: passwd.c:163
msgid "lock the password for the named account (root only)"
msgstr "паролро барои ҳисоби номбаршуда қулф кунед (танҳо root)"

#: passwd.c:166
msgid "unlock the password for the named account (root only)"
msgstr "пароли корбари номбаршударо кушоед (танҳо root)"

#: passwd.c:169
msgid "expire the password for the named account (root only)"
msgstr "мӯҳлати пароли корбари номбаршударо ба итмом расед (танҳо root)"

#: passwd.c:172
msgid "force operation"
msgstr "амалиёти маҷбурӣ"

#: passwd.c:174
msgid "maximum password lifetime (root only)"
msgstr "вақти аксари мавҷудияти парол(танҳо root)"

#: passwd.c:176
msgid "minimum password lifetime (root only)"
msgstr "вақти ақали мавҷудияти парол (танҳо root)"

#: passwd.c:178
msgid ""
"number of days warning users receives before password expiration (root only)"
msgstr ""
"миқдори рӯзҳои огоҳие, ки корбарон қабл аз ба охир расидани муддати парол "
"(танҳо root) қабул мекунанд"

#: passwd.c:181
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr ""
"миқдори рӯзҳои баъди ба охир расидани муддати парол вақте, ки ҳисоб бекор "
"карда мешавад (танҳо root)"

#: passwd.c:184
msgid "report password status on the named account (root only)"
msgstr "ҳолати паролро дар ҳисоби номбаршуда (танҳо root) ҳисобот диҳед"

#: passwd.c:187
msgid "read new tokens from stdin (root only)"
msgstr "рамзҳои навро аз stdin хонед (танҳо root)"

#: passwd.c:194
msgid "[OPTION...] <accountName>"
msgstr "[ИМКОНОТ...] <Номи ҳисоб>"

#: passwd.c:197
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: далели бад %s: %s\n"

#: passwd.c:258
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s: Танҳо яке аз -l, -u, -d, -S метавонед таъин кунед.\n"

#: passwd.c:267
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr ""
"%s: Яке аз  -l, -u, -d, -S ва дигаре аз -i, -n, -w, -x омехта карда "
"намешавад.\n"

#: passwd.c:286
#, c-format
msgid "Only root can do that.\n"
msgstr "Танҳо root метавонад ин амалро иҷро кунад.\n"

#: passwd.c:299
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: Танҳо root метавонад номи корбарро таъин кунад.\n"

#: passwd.c:308
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: Номи корбари пешниҳодшуда аз ҳад дароз аст.\n"

#: passwd.c:318
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: Танҳо як номи корбар метавонад таъин карда шавад.\n"

#: passwd.c:328
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: Барои имконоти зерин номи корбар лозим аст.\n"

#: passwd.c:339
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: Шуморо муайян накард!\n"

#: passwd.c:348 passwd.c:383
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: Номи корбари номаълум '%s'.\n"

#: passwd.c:392
#, c-format
msgid "%s: SELinux denying access due to security policy.\n"
msgstr "%s: Бинобар аз сиёсати амният SELinux дастрасиро қатъ мекунад.\n"

#: passwd.c:402
#, c-format
msgid "Locking password for user %s.\n"
msgstr "Бастани парол барои корбари %s.\n"

#: passwd.c:406 passwd.c:418 passwd.c:433 passwd.c:444 passwd.c:464
msgid "Success"
msgstr "Муваффақият"

#: passwd.c:406 passwd.c:421
msgid "Error (password not set?)"
msgstr "Хато (паролро сабт кардед?)"

#: passwd.c:414
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "Кушодани парол барои корбари %s.\n"

#: passwd.c:420
msgid "Unsafe operation (use -f to force)"
msgstr "Амалиёти беэътибор (истифодаи -f барои тарзи маҷбурӣ)"

#: passwd.c:429
#, c-format
msgid "Expiring password for user %s.\n"
msgstr "Мӯҳлати пароли корбари %s ба итмом мерасад.\n"

#: passwd.c:433 passwd.c:444 passwd.c:464
msgid "Error"
msgstr "Хато"

#: passwd.c:441
#, c-format
msgid "Removing password for user %s.\n"
msgstr "Тозакунии парол барои корбари %s.\n"

#: passwd.c:461
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "Танзими санаи муддат барои корбари %s.\n"

#: passwd.c:477
#, c-format
msgid "Changing password for user %s.\n"
msgstr "Ивазкунии парол барои корбари %s.\n"

#: passwd.c:489
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: хатогии хониш аз stdin: %s\n"

#: passwd.c:506
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: оғоз кардани pam имкон нест: %s\n"

#: passwd.c:519
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: ба pam танзим додани tty имкон нест: %s\n"

#: passwd.c:530
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: танзими рад кардани монондан дастрас нест: %s\n"

#: passwd.c:548
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr ""
"%s: рамзҳои аз муддат гузаштаи санҷиши аслият бо муввафақият танзим шудаанд."
"\n"

#: passwd.c:554
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: ҳама рамзҳои санҷиши аслият бо муввафқият танзим дода шудаанд.\n"
