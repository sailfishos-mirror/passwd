#: libuser.c:90
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s：libuser初始化错误："

#: libuser.c:167
msgid "Warning: unlocked password would be empty."
msgstr "警告：未锁定的口令将是空的。"

#: libuser.c:281
msgid "Corrupted passwd entry."
msgstr "错误的passwd条目。"

#: libuser.c:298
msgid "Empty password."
msgstr "口令为空。"

#: libuser.c:301
msgid "Password locked."
msgstr "口令已被锁定。"

#: libuser.c:305
msgid "Password set, MD5 crypt."
msgstr "口令已设置，使用MD5加密。"

#: libuser.c:308
msgid "Password set, blowfish crypt."
msgstr "口令已设置，使用blowfish进行加密。"

#: libuser.c:310
msgid "Password set, unknown crypt variant."
msgstr "口令已设置，加密方式未知。"

#: libuser.c:313
msgid "Alternate authentication scheme in use."
msgstr "更改当前使用的认证方案。"

#: libuser.c:318
msgid "Password set, DES crypt."
msgstr "口令已设置，使用DES进行加密。"

#: libuser.c:332
#, c-format
msgid "No password set.\n"
msgstr "口令未设置。\n"

#: libuser.c:336
#, c-format
msgid "Unknown user.\n"
msgstr "未知用户。\n"

#: libuser.c:431
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s： 用户帐号不支持口令老化功能。\n"

#: passwd.c:158
msgid "keep non-expired authentication tokens"
msgstr "保持身份验证令牌不过期"

#: passwd.c:160
msgid "delete the password for the named account (root only)"
msgstr "删除已命名帐号的口令（只有根用户才能进行此操作）"

#: passwd.c:162
msgid "lock the named account (root only)"
msgstr "锁定已命名的帐号（只有根用户才能进行此操作）"

#: passwd.c:164
msgid "unlock the named account (root only)"
msgstr "解锁已命名的帐号（只有根用户才能进行此操作）"

#: passwd.c:166
msgid "force operation"
msgstr "强制执行操作"

#: passwd.c:168
msgid "maximum password lifetime (root only)"
msgstr "口令的最长有效时限（只有根用户才能进行此操作）"

#: passwd.c:170
msgid "minimum password lifetime (root only)"
msgstr "口令的最短有效时限（只有根用户才能进行此操作）"

#: passwd.c:172
msgid ""
"number of days warning users receives before password expiration (root only)"
msgstr "在口令过期前多少天开始提醒用户（只有根用户才能进行此操作）"

#: passwd.c:175
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr "当口令过期后经过多少天该帐号会被禁用（只有根用户才能进行此操作）"

#: passwd.c:178
msgid "report password status on the named account (root only)"
msgstr "报告已命名帐号的口令状态（只有根用户才能进行此操作）"

#: passwd.c:180
msgid "read new tokens from stdin (root only)"
msgstr "从标准输入读取令牌（只有根用户才能进行此操作）"

#: passwd.c:187
msgid "[OPTION...] <accountName>"
msgstr "[选项...] <帐号名称>"

#: passwd.c:190
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s：错误的参数 %s： %s\n"

#: passwd.c:248
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s： 参数 -l， -u， -d， -S只能指定其中的一个。\n"

#: passwd.c:257
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr ""
"%s：参数 -l， -u， -d， -S 不能和参数 -i， -n， -w， -x中的任一个选项合用。\n"

#: passwd.c:281
#, c-format
msgid "Only root can do that.\n"
msgstr "只有根用户才能进行此操作。\n"

#: passwd.c:294
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s： 只有根用户才能指定用户名称。\n"

#: passwd.c:303
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s：提供的用户名称长度太长。\n"

#: passwd.c:313
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s：只能指定一个用户的名称。\n"

#: passwd.c:323
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s： 该选项需要使用用户名称。\n"

#: passwd.c:334
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s： 无法确认您的身份！\n"

#: passwd.c:343 passwd.c:378
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s： 未知的用户名称 '%s'。\n"

#: passwd.c:395
#, c-format
msgid "%s: %s is not authorized to change the password of %s\n"
msgstr "%s： %s 无权限更改 %s 的口令 \n"

#: passwd.c:408
#, c-format
msgid "Locking password for user %s.\n"
msgstr "锁定用户 %s 的口令 。\n"

#: passwd.c:412 passwd.c:424 passwd.c:438 passwd.c:458
msgid "Success"
msgstr "操作成功"

#: passwd.c:412 passwd.c:427
msgid "Error (password not set?)"
msgstr "错误（口令未设置？）"

#: passwd.c:420
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "解锁用户 %s 的口令 。\n"

#: passwd.c:426
msgid "Unsafe operation (use -f to force)"
msgstr "不安全的操作（使用 -f 参数强制进行该操作）"

#: passwd.c:435
#, c-format
msgid "Removing password for user %s.\n"
msgstr "清除用户的口令 %s。\n"

#: passwd.c:438 passwd.c:458
msgid "Error"
msgstr "错误"

#: passwd.c:455
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "调整用户口令老化数据%s。\n"

#: passwd.c:471
#, c-format
msgid "Changing password for user %s.\n"
msgstr "更改用户 %s 的口令 。\n"

#: passwd.c:483
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s：从标准输入：%s 读取时发生错误。 \n"

#: passwd.c:500
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s：不能启动pam： %s\n"

#: passwd.c:513
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s：不能为pam设置tty： %s\n"

#: passwd.c:524
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s：不能够设置失败延迟： %s\n"

#: passwd.c:542
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s：过期的身份验证令牌已经成功更新。\n"

#: passwd.c:548
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s： 所有的身份验证令牌已经成功更新。\n"
