#: libuser.c:90
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: помилка ініціализації libuser:"

#: libuser.c:167
msgid "Warning: unlocked password would be empty."
msgstr "Попередження: розблокований пароль буде порожнім."

#: libuser.c:281
msgid "Corrupted passwd entry."
msgstr "Пошкоджений запис у passwd."

#: libuser.c:298
msgid "Empty password."
msgstr "Порожній пароль."

#: libuser.c:301
msgid "Password locked."
msgstr "Пароль заблоковано."

#: libuser.c:305
msgid "Password set, MD5 crypt."
msgstr "Пароль заданий, шифр MD5."

#: libuser.c:308
msgid "Password set, blowfish crypt."
msgstr "Пароль заданий, шифр blowfish."

#: libuser.c:310
msgid "Password set, unknown crypt variant."
msgstr "Пароль заданий, шифр невідомий."

#: libuser.c:313
msgid "Alternate authentication scheme in use."
msgstr "Використовується альтернативна схема перевірки автентичності."

#: libuser.c:318
msgid "Password set, DES crypt."
msgstr "Пароль заданий, шифр DES."

#: libuser.c:332
#, c-format
msgid "No password set.\n"
msgstr "Пароль не заданий.\n"

#: libuser.c:336
#, c-format
msgid "Unknown user.\n"
msgstr "Невідомий користувач.\n"

#: libuser.c:431
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s: обліковий запис не підтримує закінчення терміну дії паролю.\n"

#: passwd.c:158
msgid "keep non-expired authentication tokens"
msgstr "зберегти не застарілі ознаки перевірки автентичності (паролі)"

#: passwd.c:160
msgid "delete the password for the named account (root only)"
msgstr "видалити пароль для облікового рахунку (дозволено лише root)"

#: passwd.c:162
msgid "lock the named account (root only)"
msgstr "заблокувати обліковий запис (дозволено лише користувачу root)"

#: passwd.c:164
msgid "unlock the named account (root only)"
msgstr "розблокувати обліковий запис (дозволено лише користувачу root)"

#: passwd.c:166
msgid "force operation"
msgstr "примусове виконання"

#: passwd.c:168
msgid "maximum password lifetime (root only)"
msgstr "максимальний термін дії паролю (дозволено лише користувачу root)"

#: passwd.c:170
msgid "minimum password lifetime (root only)"
msgstr "мінімальний термін дії паролю (дозволено лише користувачу root)"

#: passwd.c:172
msgid ""
"number of days warning users receives before password expiration (root only)"
msgstr ""
"за скільки днів до дати закінчення терміну дії паролю почати попереджувати користувача "
"(дозволено лише користувачу root)"

#: passwd.c:175
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr ""
"через скільки днів після закінчення терміну дії паролю блокувати обліковий запис "
"(дозволено лише користувачу root)"

#: passwd.c:178
msgid "report password status on the named account (root only)"
msgstr "зберегти стан паролю для облікового запису (дозволено лише користувачу root)"

#: passwd.c:180
msgid "read new tokens from stdin (root only)"
msgstr "прочитати нове значення зі стандартного потоку вводу (дозволено лише користувачу root)"

#: passwd.c:187
msgid "[OPTION...] <accountName>"
msgstr "[ПАРАМЕТРИ...] <обліковий_запис>"

#: passwd.c:190
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: неправильний аргумент %s: %s\n"

#: passwd.c:248
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr ""
"%s: Допускається вказувати лише один з параметрів: -l, -u, -d чи -S.\n"

#: passwd.c:257
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr "%s: Не допускається змішувати ключі -l, -u, -d, -S и -i, -n, -w, -x.\n"

#: passwd.c:281
#, c-format
msgid "Only root can do that.\n"
msgstr "Операція дозволена лише користувачу root.\n"

#: passwd.c:294
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: Лише root може вказувати назву облікового запису.\n"

#: passwd.c:303
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: Вказана назва користувача надто довга.\n"

#: passwd.c:313
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: Допускається вказувати лише одну назву.\n"

#: passwd.c:323
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: Для цього параметра необхідне ім'я користувача.\n"

#: passwd.c:334
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: Не вдається вас ідентифікувати!\n"

#: passwd.c:343 passwd.c:378
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: Невідоме ім'я '%s'.\n"

#: passwd.c:395
#, c-format
msgid "%s: %s is not authorized to change the password of %s\n"
msgstr "%s: %s не дозволяється міняти пароль для %s\n"

#: passwd.c:408
#, c-format
msgid "Locking password for user %s.\n"
msgstr "Блокується пароль для користувача %s.\n"

#: passwd.c:412 passwd.c:424 passwd.c:438 passwd.c:458
msgid "Success"
msgstr "Успішно виконано"

#: passwd.c:412 passwd.c:427
msgid "Error (password not set?)"
msgstr "Помилка (пароль не заданий?)"

#: passwd.c:420
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "Знімається блокування паролю для користувача %s.\n"

#: passwd.c:426
msgid "Unsafe operation (use -f to force)"
msgstr "Операція небезпечна (використовуйте -f для примусового виконання)"

#: passwd.c:435
#, c-format
msgid "Removing password for user %s.\n"
msgstr "Видаляється пароль для користувача %s.\n"

#: passwd.c:438 passwd.c:458
msgid "Error"
msgstr "Помилка"

#: passwd.c:455
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "Встановлюються параметри закінчення терміну дії для користувача %s.\n"

#: passwd.c:471
#, c-format
msgid "Changing password for user %s.\n"
msgstr "Заміна паролю для користувача %s.\n"

#: passwd.c:483
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: помилка читання зі стандартного потоку вводу: %s\n"

#: passwd.c:500
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: небезпечно запускати pam: %s\n"

#: passwd.c:513
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: не вдається задати tty для pam: %s\n"

#: passwd.c:524
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: не вдається вказати затримку при помилці: %s\n"

#: passwd.c:542
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s: успішно оновлено застарілу ознаку перевірки автентичності (пароль).\n"

#: passwd.c:548
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: всі ознаки перевірки автентичності (паролі) успішно оновлені.\n"
